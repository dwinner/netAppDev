on timer systemtimetimer
{
   char systemtime [7];
   long datetime[9];
   int i;
   char temp[3];

	settimer(systemtimetimer, 1000);

	for(i=0;i<3;i++)
		temp[i]=0;

	getlocaltime(datetime);	

	//hours
	ltoa(datetime[2], temp, 10);

	if(datetime[2]<10){
		systemtime[0]='0';
		systemtime[1]=temp[0];
	}
	else{
		systemtime[0]=temp[0];
		systemtime[1]=temp[1];
	}

	//minutes
	for(i=0;i<3;i++)
		temp[i]=0;

	ltoa(datetime[1], temp, 10);

	if(datetime[1]<10){
		systemtime[2]='0';
		systemtime[3]=temp[0];
	}
	else{
		systemtime[2]=temp[0];
		systemtime[3]=temp[1];
	}

	//seconds
	for(i=0;i<3;i++)
		temp[i]=0;

	ltoa(datetime[0], temp, 10);

	if(datetime[0]<10){
		systemtime[4]='0';
		systemtime[5]=temp[0];
	}
	else{
		systemtime[4]=temp[0];
		systemtime[5]=temp[1];
	}
	
	putvalue(env_FSG_time, systemtime);
}

