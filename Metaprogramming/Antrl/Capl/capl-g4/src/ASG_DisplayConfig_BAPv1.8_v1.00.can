    } 

    controls_enable_all();
}

int Array_getNextElementPos(byte validity[], int startPos, byte direction)
{
	int i;
	int startPosBackup;
	
	startPosBackup = startPos;
	
	if(direction == 0)
	{
		for(i=1; i+startPos<elCount(validity); i++)
			if(validity[i+startPos] != 0)
				return (i+startPos);
	}
	else
	{
		for(i=1; startPos-i >= 0; i++)
			if(validity[startPos-i] != 0)
				return (startPos-i);
    }
	return -1;
}

int Array_getTNLE(byte validity[])
{
	int i;
	int tnle;
	tnle = 0;
	
	for(i=0; i<elCount(validity); i++)
		if(validity[i] != 0)
			tnle++;
		
	return tnle;
}

int Array_getRequestedIndices (word buffer[], int startPos, byte direction, byte shift, int tnle, int elementsRequested, byte validity[])
{
    int nextEl;
    int numberOfElements;
    int i;
    numberOfElements = 0;

	if(validity[startPos] != 0)
		nextEl = startPos;
	else
	    nextEl = Array_getNextElementPos (validity, startPos, direction);
	// evaluate shift
	if(shift && nextEl == startPos)
		nextEl = Array_getNextElementPos (validity, nextEl, direction);
	else if(shift && startPos == 0)
		nextEl = Array_getNextElementPos_withWrap (validity, nextEl, direction);

    if(nextEl != -1)
    {
		buffer[numberOfElements] = nextEl;
		numberOfElements++;

		for(i=1; i<elementsRequested && i<elCount(validity); i++)
		{
			nextEl = Array_getNextElementPos (validity, nextEl, direction);
			if(nextEl == -1)
				break;
			else
			{
				buffer[numberOfElements] = nextEl;
				numberOfElements++;
            }
		}
    }

    return numberOfElements;
}

int Array_getNextElementPos_withWrap(byte validity[], int startPos, byte direction)
{
	int i;
	int startPosBackup;
	int nextPos;
	
	startPosBackup = startPos;
	
	nextPos = Array_getNextElementPos(validity, startPos, direction);

	if(nextPos == -1)
	{
		if(direction == 0)
			startPos = -1;
		else
			startPos = elCount(validity);

		nextPos = Array_getNextElementPos(validity, startPos, direction);
			if(nextPos != startPosBackup)	// catch if wrap around occured and same element is referenced
				return (nextPos);
    }
	
	return nextPos;
}

int Array_getFirstElementPos(byte validity[], byte direction)
{
	int i;
	
	if(direction == 0)
	{
		for(i=0; i<elCount(validity); i++)
			if(validity[i] != 0)
				return i;
	}
	else
	{
		for(i=elCount(validity)-1; i>=0; i--)
			if(validity[i] != 0)
				return i;
    }
	
	return -1;
}

