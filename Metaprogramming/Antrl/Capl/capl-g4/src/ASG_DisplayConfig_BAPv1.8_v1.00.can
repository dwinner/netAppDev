				break;
		
			case DISPLAY_2_SETUP_FCT_ID:
				Indication_0x15_Display2Setup(indicationHeader, indicationData, datalength);
				break;
		
			case DISPLAY_2_MAIN_SELECTION_FCT_ID:
				Indication_0x16_Display2MainSelection(indicationHeader, indicationData, datalength);
				break;
		
			case DISPLAY_3_SETUP_FCT_ID:
				Indication_0x17_Display3Setup(indicationHeader, indicationData, datalength);
				break;
		
			case DISPLAY_3_MAIN_SELECTION_FCT_ID:
