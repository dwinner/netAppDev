ElementContentSelection_init_CSV_setGet () //Byte & file selection
{
    int i = 0;
    int j = 0;
    int k = 0;

    byte dataBuffer[5000];
    int byteCount = 0;
    char file_complete[5000];

    // byte buffers to store characters
    char buffer_pos[5];
    char buffer_display[5];
    char buffer_additionalInfo[5];
	char buffer_element[5];
	char buffer_elementContent[5];
    int buffer_incrementator = 0;

    // differentiate data fields
    enum dataTypeEnum {pos, display, additionalInfo, element, elementContent};
    int dataType = pos;
    int inc_csv_line;
    inc_csv_line = 0;

    // catch if no file was selected
    if(sysGetVariableArrayLength(sysvar::DisplayConfig_ASG_ElementContentSelection::x12_ElementContentSelection_control_externalFilePath) == 0)
    {
        sysSetVariableString(sysvar::DisplayConfig_ASG_ElementContentSelection::x12_ElementContentSelection_control_loadReload_result, "Select file first!");
        @sysvar::DisplayConfig_ASG_ElementContentSelection::x12_ElementContentSelection_control_default_external    = 0;
        return;
    }

    // Get complete path
    sysGetVariableString(sysvar::DisplayConfig_ASG_ElementContentSelection::x12_ElementContentSelection_control_externalFilePath, file_complete, elCount(file_complete));

    byteCount = File_readCSV (dataBuffer, file_complete);
	
    // clear old data
    ElementContentSelection_clearData_setGet();

    /*** Parse data and create new ElementContentSelection ***/            
    for(i=0; i<byteCount; i++)
    {
        // nextline in *.csv -> "LF" and "CR" detected
        if( (dataBuffer[i] == 0x0D) && (dataBuffer[i+1] == 0x0A) )
        {
            // Take data if not in first line
            if(inc_csv_line > 0)
            {
                ElementContentSelection_Data_Pos_setGet[atol(buffer_pos)] = atol(buffer_pos);
				ElementContentSelection_Data_Validity_setGet[atol(buffer_pos)] = 1;
                ElementContentSelection_Data_Display_setGet[atol(buffer_pos)] = atol(buffer_display);
				ElementContentSelection_Data_AdditionalInfo_setGet[atol(buffer_pos)] = atol(buffer_additionalInfo);
				ElementContentSelection_Data_Element_setGet[atol(buffer_pos)] = atol(buffer_element);
				ElementContentSelection_Data_ElementContent_setGet[atol(buffer_pos)] = atol(buffer_elementContent);
            }

            inc_csv_line++;
            i++;                        // skip CR
            dataType = pos;             // starts with pos again
            buffer_incrementator = 0;   // first buffer position

            // clear all buffers
            for(j=0; j<elCount(buffer_pos); j++)
                buffer_pos[j] = 0;
            for(j=0; j<elCount(buffer_display); j++)
                buffer_display[j] = 0;
            for(j=0; j<elCount(buffer_additionalInfo); j++)
                buffer_additionalInfo[j] = 0;
			for(j=0; j<elCount(buffer_element); j++)
                buffer_element[j] = 0;
			for(j=0; j<elCount(buffer_elementContent); j++)
                buffer_elementContent[j] = 0;
        }
        else
        // Don't work in header line
        if(inc_csv_line > 0)
        {
            // detection of ';'
            if (dataBuffer[i] == 0x3B)
            {
                dataType++;
                buffer_incrementator = 0;
            }
            else // sign to read
            {
                switch(dataType)
                {
                    case pos:
                        buffer_pos[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;

                    case display:
                        buffer_display[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;

                    case additionalInfo:
                        buffer_additionalInfo[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case element:
                        buffer_element[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
                    
					case elementContent:
                        buffer_elementContent[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;

                    default:    // undefined / error
                        writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR,"Fehler beim Lesen der .csv-Datei! Ungültiger Datentyp: %d", dataType);
                        break;
                }
            }
        }
    }

	sysSetVariableString(sysvar::DisplayConfig_ASG_ElementContentSelection::x12_ElementContentSelection_control_loadReload_result, "File successfully loaded!");

	ElementContentSelection_Data_startPos_setGet = Array_getFirstElementPos(ElementContentSelection_Data_Validity_setGet, 0);
    ElementContentSelection_actualize_sysvar_setGet ();

    enableControl("DisplayConfig_ASG_Array_ElementContentSelection", "btn_dataSource_external", TRUE);
}

File_separatePath (char completePath[], char path[], char file[])
{
    int i, j;
    int separators;
    int separatorCounter;
    int fileName_length;

    // clear buffer
    for(i=0; i<elCount(path); i++)
        path[i] = 0;
    for(i=0; i<elCount(file); i++)
        file[i] = 0;

    // Count separators
    i = 0;
    separators = 0;
    while (completePath[i] !=0)
    {
        if(completePath[i] == 0x5C)
            separators++;                
        i++;
    } 

    // Get position of last separator
    i = 0;
    separatorCounter = 0;
    while (separatorCounter != separators)
    {
        if(completePath[i] == 0x5C)
            separatorCounter++;                
        i++;
    }

    // Copy path to destination (stop on last separator position)
    strncpy(path, completePath, i);

    // get length of filename
    j = i;
    fileName_length = 0;
    while(completePath[j] != 0x00)
    {
        fileName_length++;
        j++;
    }

    // Copy filename to destination (start after last separator)
    for(j=0; j<fileName_length; j++)
        file[j] = completePath[i+j];
}

int File_readCSV (byte dataBuffer[], char file_complete[])
{
    char file_fileName[5000];
    char file_path[5000];
    dword CSVfileHandle = 0;

    int csv_byte_len = 0;

    File_separatePath (file_complete, file_path, file_fileName);

    //set path for *.csv file
    setFilePath(file_path, 0); 
    //open *.csv (binary-mode -> get the ASCII/UTF-8 code)
    CSVfileHandle = openFileRead(file_fileName,1); 
    if (CSVfileHandle == 0) //*.csv file access successful
        writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR,"Fehler beim Dateizugriff auf '%s'",file_complete);

    csv_byte_len = fileGetBinaryBlock (dataBuffer, elcount(dataBuffer), CSVfileHandle); // get data from *.csv and store the total number of byte in *.csv-file

    // close file
    if(fileClose (CSVfileHandle) == 0) // close not successful
        writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR,"Datei '%s' geschlossen", file_complete); // output result

    return csv_byte_len;
}

