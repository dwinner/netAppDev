				case DISPLAY_2_SETUP_FCT_ID:
                    datatype = DISPLAY_2_SETUP_BAP_DATA_TYPE;
					length = DISPLAY_2_SETUP_BAP_DATA_LENGTH;
                    break;

				case DISPLAY_2_MAIN_SELECTION_FCT_ID:
                    datatype = DISPLAY_2_MAIN_SELECTION_BAP_DATA_TYPE;
					length = DISPLAY_2_MAIN_SELECTION_BAP_DATA_LENGTH;
                    break;

				case DISPLAY_3_SETUP_FCT_ID:
