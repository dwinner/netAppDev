		idx++;
		buffer[idx] = (DisplayPresets_Data_Display2AdditionalInfo2_2_setGet[entry] & 0xFF000000) >> 24;
		idx++;
    }

    // ElementContent - if transmission is enabled via recordAddress
    if(recordAddress == DISPLAY_PRESETS_RECADDR_DISPLAY123)
    {
        buffer[idx] = (DisplayPresets_Data_Display3MainSelection_setGet[entry] & 0x000000FF) >> 0;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3MainSelection_setGet[entry] & 0x0000FF00) >> 8;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3MainSelection_setGet[entry] & 0x00FF0000) >> 16;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3MainSelection_setGet[entry] & 0xFF000000) >> 24;
		idx++;
		
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo1_1_setGet[entry] & 0x000000FF) >> 0;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo1_1_setGet[entry] & 0x0000FF00) >> 8;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo1_1_setGet[entry] & 0x00FF0000) >> 16;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo1_1_setGet[entry] & 0xFF000000) >> 24;
		idx++;
		
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo1_2_setGet[entry] & 0x000000FF) >> 0;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo1_2_setGet[entry] & 0x0000FF00) >> 8;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo1_2_setGet[entry] & 0x00FF0000) >> 16;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo1_2_setGet[entry] & 0xFF000000) >> 24;
		idx++;
		
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo2_1_setGet[entry] & 0x000000FF) >> 0;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo2_1_setGet[entry] & 0x0000FF00) >> 8;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo2_1_setGet[entry] & 0x00FF0000) >> 16;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo2_1_setGet[entry] & 0xFF000000) >> 24;
		idx++;
		
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo2_2_setGet[entry] & 0x000000FF) >> 0;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo2_2_setGet[entry] & 0x0000FF00) >> 8;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo2_2_setGet[entry] & 0x00FF0000) >> 16;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display3AdditionalInfo2_2_setGet[entry] & 0xFF000000) >> 24;
		idx++;
    }

    return idx;   
}

