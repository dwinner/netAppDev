int BAP_DLL_Request (byte request, dword Request_header[], byte Request_data[])
{
    int ret;                                                                                // Return value
    int i;

    Request_header[2] = request;
    
    if(elCount(Request_header)!= BAP_HEADER_SIZE)			                                // catch wrong header size
    {
        writeLineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "ASG: Wrong BAP Header size in BAP_DLL_Request (dword Request_header[], byte Request_data[])! Not %d but %d bytes long!", BAP_HEADER_SIZE, elCount(Request_header));
        return -1;
    }

    ret = Bap180_Request(NODE_INDEX, Request_header, Request_data);

    if(ret == 0x23) // BAP Stack Busy
    {
        if(Request_header[1] != 0x01)   // Hide GetAll (multiple calls by stack)
            writeLineEx(gBAP_Trace, CAPL_WRITE_LINE_WARNING, "ASG: BAP-Stack busy for Fct. 0x%x!", Request_header[1]);
    } else
    if(ret != 0)
    {
        writeLineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "ASG: BAP-Error in BAP_DLL_Request (dword Request_header[], byte Request_data[])! ErrorCode = 0x%x!", ret);

        for(i=0; i<elCount(Request_header); i++)
            writeLineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "Request_header[%d]=0x%x", i, Request_header[i]);
        for(i=0; i<Request_header[4]; i++)
            writeLineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "Request_data[%d]=0x%x", i, Request_data[i]);
    }
	
    return ret;
}

int ElementContentSelection_addArrayElementData (byte buffer[], int idx, int entry, int transmitPos, int recordAddress, int idxSize)
{
    int i;

    byte pos_upperByte, pos_lowerByte;
    byte byteBuffer;
    pos_upperByte = 0;
    pos_lowerByte = 0;

    // POS - if transmission is enabled via recordAddress or transmitPos
    if(transmitPos == 1 || recordAddress == ELEMENT_CONTENT_SELECTION_RECADDR_POS)
    {
        if(idxSize == 0)    // 8 bit
        {
            buffer[idx] = ElementContentSelection_Data_Pos_setGet[entry];
            idx++;
        }
        else                // 16 bit
        {
            pos_lowerByte = (ElementContentSelection_Data_Pos_setGet[entry] & 0x00FF);
            pos_upperByte = (ElementContentSelection_Data_Pos_setGet[entry] & 0xFF00) >> 8;
            buffer[idx] = pos_lowerByte;
            idx++;
            buffer[idx] = pos_upperByte;
            idx++;
        }
    }
    
    // Display, AdditionalInfo, Element - if transmission is enabled via recordAddress
    if(recordAddress == ELEMENT_CONTENT_SELECTION_RECADDR_ALL)
    {
		
		byteBuffer = 0;
		byteBuffer |= (ElementContentSelection_Data_Display_setGet[entry]		 	<< 4) & 0xF0;
		byteBuffer |= (ElementContentSelection_Data_AdditionalInfo_setGet[entry]	<< 0) & 0x0F;
		buffer[idx] = byteBuffer;
		idx++;
		
		buffer[idx] = ElementContentSelection_Data_Element_setGet[entry];
		idx++;
    }

    // ElementContent - if transmission is enabled via recordAddress
    if(recordAddress == ELEMENT_CONTENT_SELECTION_RECADDR_ALL || recordAddress == ELEMENT_CONTENT_SELECTION_RECADDR_EL_CONTENT)
    {
        buffer[idx] = ElementContentSelection_Data_ElementContent_setGet[entry];
		idx++;
    }

    return idx;   
}

