                    break;

				case ACTIVE_DISPLAY_PRESET_FCT_ID:
                    datatype = ACTIVE_DISPLAY_PRESET_BAP_DATA_TYPE;
					length = ACTIVE_DISPLAY_PRESET_BAP_DATA_LENGTH;
                    break;

				case DISPLAY_VIEW_CONFIGURATION_FCT_ID:
                    datatype = DISPLAY_VIEW_CONFIGURATION_BAP_DATA_TYPE;
					length = DISPLAY_VIEW_CONFIGURATION_BAP_DATA_LENGTH;
                    break;

				case LED_CONFIGURATION_FCT_ID:
                    datatype = LED_CONFIGURATION_BAP_DATA_TYPE;
					length = LED_CONFIGURATION_BAP_DATA_LENGTH;
                    break;
                
				default:
                    writeLineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "Unknown Fct-ID in BAP_createHeader (dword FctID): %d", FctID);
            }
            break;

        default:
            writeLineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "Unhandled RequestType in BAP_createHeader (dword FctID): %d", requestType);
    }

    // Create header
    headerBuffer[0] = LSG_DisplayConfig;			// LSG-ID
	headerBuffer[1] = FctID;		                // Fct-ID
	headerBuffer[2] = requestType;					// BAP requestType
	headerBuffer[3] = datatype;		                // BAP data-type (BapDataType_t)
	headerBuffer[4] = length;				    	// Length for data type byte sequence
	headerBuffer[5] = datalength;                       		// data (uint8/uint16/uint32 or Errorcode) or datalength
}

