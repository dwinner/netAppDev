		        break;

            case FSG_OPERATION_STATE_FCT_ID:
			    Indication_0x0F_FSG_OperationState(indicationHeader, indicationData, datalength);
		        break;

			case BRIGHTNESS_FCT_ID:
				Indication_0x10_Brightness(indicationHeader, indicationData, datalength);
				break;
		
			case VOLUME_FCT_ID:
				Indication_0x11_Volume(indicationHeader, indicationData, datalength);
				break;
		
			case ELEMENT_CONTENT_SELECTION_FCT_ID:
				Indication_0x12_ElementContentSelection(indicationHeader, indicationData, datalength);
				break;
		
			case DISPLAY_1_SETUP_FCT_ID:
				Indication_0x13_Display1Setup(indicationHeader, indicationData, datalength);
				break;
		
			case DISPLAY_1_MAIN_SELECTION_FCT_ID:
