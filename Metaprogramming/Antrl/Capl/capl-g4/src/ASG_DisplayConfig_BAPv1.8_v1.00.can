void Indication_0x12_ElementContentSelection(dword indication_header [], byte indication_data[], int datalength)
{
    int tnle;
    int i;

    tnle = @sysvar::DisplayConfig_ASG_ElementContentSelection::x12_ElementContentSelection_TNLE;

    if(!@sysvar::DisplayConfig_ASG_ElementContentSelection::x12_ElementContentSelection_control_onOff)
        return;

    switch (indication_header[2]) // indication
	{
		case Changed_IND:
            // Clear old Array data
            ElementContentSelection_clearData_get();

            // Get First Element
            ElementContentSelection_getElement(0);
		    break;

        case Data_IND:
			// if Status Array with MSB of ASG_ID == 1 -> broadcast -> handle as changedArray
			if(Array_DecodeFirstByte_getASGID(indication_data[0]) & 0x8 && indication_header[3] == Bap_ByteSequence)	// Don't listen to Acknowledge
			{
				// Clear old Array data
				ElementContentSelection_clearData_get();
				// Get First Element
				ElementContentSelection_getElement(0);
				return;
			}
				
            // Check right Addressing 
            if(Array_DecodeFirstByte_getASGID(indication_data[0]) != ELEMENT_CONTENT_SELECTION_ASGID                      	// Only evaluate messages with valid ASGID
            || Array_DecodeFirstByte_getTAID(indication_data[0]) != ElementContentSelection_Data_lastRequestedTAID)     	// Only evaluate correct answered GetArrays
                return;
			
            // tnle == 0
            if(tnle >= indication_data[1] )                                             
                return;
			
            // Check if Get-Parameters are OK
            if(Array_DecodeModeByte_getShift(indication_data[2])            != 0                            				// No Shift
            || Array_DecodeModeByte_getDirection(indication_data[2])        != 0                            				// Forwards
            || Array_DecodeModeByte_getIdxSize(indication_data[2])          != 0                            				// 8 Bit
            || Array_DecodeModeByte_getRecordAddress(indication_data[2])    != ELEMENT_CONTENT_SELECTION_RECADDR_ALL  		// Full data
            || indication_data[4]                                           != 1)                           				// Elements
                return;
			
			// Everything OK, take Data
			extractData_0x12_ElementContentSelection(indication_data);

            // Get next element, if tnle>this
			tnle = indication_data[1];    
            if(@sysvar::DisplayConfig_ASG_ElementContentSelection::x12_ElementContentSelection_TNLE < tnle)
                ElementContentSelection_getElement(indication_data[5] + 1);

		    break;

		case Error_IND:
            writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "DisplayConfig_ASG: Error-Indication in Indication_0x12_ElementContentSelection: Error 0x%x", indication_header[5]);
		    break;
		
        default:
			writelineEx(gBAP_Trace, CAPL_WRITE_LINE_INFORMATION, "DisplayConfig_ASG: Unhandled indication 0x%x in Indication_0x12_ElementContentSelection.", indication_header[2]);
		    break;
	} 
}

