int DisplayPresets_addArrayElementData (byte buffer[], int idx, int entry, int transmitPos, int recordAddress, int idxSize)
{
    int i;

    byte pos_upperByte, pos_lowerByte;
    byte byteBuffer;
    pos_upperByte = 0;
    pos_lowerByte = 0;

    // POS - if transmission is enabled via recordAddress or transmitPos
    if(transmitPos == 1 || recordAddress == DISPLAY_PRESETS_RECADDR_POS)
    {
        if(idxSize == 0)    // 8 bit
        {
            buffer[idx] = DisplayPresets_Data_Pos_setGet[entry];
            idx++;
        }
        else                // 16 bit
        {
            pos_lowerByte = (ElementContentSelection_Data_Pos_setGet[entry] & 0x00FF);
            pos_upperByte = (ElementContentSelection_Data_Pos_setGet[entry] & 0xFF00) >> 8;
            buffer[idx] = pos_lowerByte;
            idx++;
            buffer[idx] = pos_upperByte;
            idx++;
        }
    }
    
    // Display, AdditionalInfo, Element - if transmission is enabled via recordAddress
    if(recordAddress == DISPLAY_PRESETS_RECADDR_DISPLAY12 || recordAddress == DISPLAY_PRESETS_RECADDR_DISPLAY123)
    {
		buffer[idx] = DisplayPresets_Data_State_setGet[entry];
		idx++;
		
		buffer[idx] = DisplayPresets_Data_DisplayPresetType_setGet[entry];
		idx++;
		
		buffer[idx] = DisplayPresets_Data_DisplayPresetInstance_setGet[entry];
		idx++;
		
		buffer[idx] = (DisplayPresets_Data_Display1MainSelection_setGet[entry] & 0x000000FF) >> 0;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display1MainSelection_setGet[entry] & 0x0000FF00) >> 8;
		idx++;
		buffer[idx] = (DisplayPresets_Data_Display1MainSelection_setGet[entry] & 0x00FF0000) >> 16;
		idx++;
