on sysvar sysvar::DisplayConfig_ASG::control_OnOff
{
	if(@this)               // Startup
    {
        init_DisplayConfig();
        setTimer(TaskTimer, 10);
        putValue(env_DisplayConfig_v100_PowerOnOff, 1); 
    }
	else                    // Must be a shutdown.	
    {
        Shutdown_DisplayConfig();
        cancelTimer(TaskTimer);
