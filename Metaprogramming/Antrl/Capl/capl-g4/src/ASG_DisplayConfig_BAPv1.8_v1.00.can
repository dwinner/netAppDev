on preStart
{
    int error;
    char Path[255];
    char BAPVersion [200];

	// create write window for BAP communication
    gBAP_Trace  = writecreate("DisplayConfig_ASG BAP-Trace");								
    writeclear(gBAP_Trace);													
	gError_Trace = writecreate("DisplayConfig_ASG Error");									
	writeclear(gError_Trace);												

    //clear CAPL-window in write-window
    writeclear(1);	

    @sysvar::DisplayConfig_General::Smart = 0;
    //get version from Stack
	Bap180_GetVersions(BAPVersion, 200);
	writelineEx(gBAP_Trace, CAPL_WRITE_LINE_SUCCESS, "*********************************************************************************");
	writelineEx(gBAP_Trace, CAPL_WRITE_LINE_SUCCESS, BAPVersion);
	writelineEx(gBAP_Trace, CAPL_WRITE_LINE_SUCCESS, "*********************************************************************************");

    // Create Node
	NODE_INDEX = Bap180_CreateNode();

    // Load DBC and XML files. 
    // load dbc-file
    getAbsFilePath(dbc_path, Path, 255);
	writelineEx(gBAP_Trace, CAPL_WRITE_LINE_SUCCESS, "ASG-DisplayConfig: Loading BAP_DBC-File: '%s'", Path);
   
	if(Bap180_LoadDBC(NODE_INDEX, 1, Path) != BAP_NO_ERROR)						
		writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "ASG-DisplayConfig Fehler beim Laden: '%s', Path");

	//load xml-file 
	getAbsFilePath(xml_path, Path, 255);
    writelineEx(gBAP_Trace, CAPL_WRITE_LINE_SUCCESS, "ASG-DisplayConfig: Load BAP-XML-File: '%s'", Path);
	
  
    if (Bap180_LoadConfig(NODE_INDEX,Path,"") != BAP_NO_ERROR) 
	    writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "ASG-DisplayConfig: Fehler beim Laden: '%s' !", Path);	   
    writelineEx(gBAP_Trace, CAPL_WRITE_LINE_SUCCESS, "*********************************************************************************");

    // BAP DLL: Add CAN channel: BAP channel 1 is mapped to CAN card channel 1.
	Bap180_AddCanMap(NODE_INDEX, 1, 1);	
    
    // BAP INIT
	error = Bap180_Init(NODE_INDEX,LSG_DisplayConfig);
    // Check for initialisation error.
    if(error == BAP_NO_ERROR)
	    writelineEx(gBAP_Trace, CAPL_WRITE_LINE_SUCCESS, "ASG-DisplayConfig BAP_Init successful");	
    else
	    writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR,"ASG-DisplayConfig error in BAP_DLL_StackStart () - BAP INIT: 0x%x", error);	
}

on start
{
    init_variables_controls();

    controls_enable_all();    
    init_variables();
    setTimer(initTimer, 3000);
}

on timer TaskTimer
{
	Bap180_Task(NODE_INDEX);	// DLL-Task
	
    BAP_DLL_Indication();
	
	if (getValue(eSendEnabled))
        {
          BAP_CAN_TX_Request();
        }

	setTimer(TaskTimer, 10);	// restart BAP-DLL-Timer
}

