    ASG_ID          = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_ASGID;
    TAID            = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_TAID_hex;

    start_pos       = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_start_hex;
    elements        = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_elements_hex;

    modeByte        = Array_createModeByte (shift, direction, transmitPos, idxSize, recordAddress);
    i = Array_createHeader_get (data, start_pos, elements, ASG_ID, TAID, modeByte, idxSize);

    BAP_createHeader(DISPLAY_PRESETS_FCT_ID, header, i, DataGet_REQ);
    BAP_DLL_Request(DataGet_REQ, header, data);
}

Request_0x1B_DisplayPresets_setGet ()
{
	// BAP stack buffer
    dword header [BAP_HEADER_SIZE];
    byte data[BAP_BUFFER_SIZE];
    word indices[ELEMENT_CONTENT_SELECTION_ENTRIES];
    int dataIterator; // data[]-iterator
    
    byte byteBuffer;
    byte modeByte;
    int i;

    // header variables
    byte recordAddress, shift, direction, transmitPos, idxSize, ASG_ID, TAID;
    word start_pos, elements, tnle;
    
    //temporary array-element variables
    int elementsCount;

    // clear variables
    for(i=0; i<elCount(header); i++)
        header[i] = 0;
    for(i=0; i<elCount(data); i++)
        data[i] = 0;
    for(i=0; i<elCount(indices); i++)
        indices[i] = 0;

    // get data
    tnle = Array_getTNLE(DisplayPresets_Data_Validity_setGet);
    recordAddress = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_recordAddress;
    shift = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_shift;
    direction = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_direction;
    transmitPos = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_transmitPos;
    idxSize = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_indexSize;
    ASG_ID = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_ASGID;
    TAID = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_TAID_hex;
    start_pos = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_start_hex;
    elements = Array_getTNLE(DisplayPresets_Data_Validity_setGet);
	
    elementsCount = Array_getRequestedIndices (indices, start_pos, direction, shift, tnle, elements, DisplayPresets_Data_Validity_setGet);
	
    // if start in header != position of array element, change TransmitPos to true
    if(elementsCount != 0)
    {
        if(@sysvar::DisplayConfig_General::Smart && start_pos != DisplayPresets_Data_Pos_setGet[indices[0]])
        {
            @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_transmitPos = 1;
            transmitPos = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_transmitPos;
        }
    }

    // if smart, change TransmitPos to true, if requested Positions are not continuous
    if(@sysvar::DisplayConfig_General::Smart && transmitPos == 0)
    {
        for(i=0; i<(elementsCount-1); i++)
        {
            if(DisplayPresets_Data_Pos_setGet[indices[i]] != (DisplayPresets_Data_Pos_setGet[indices[i+1]]-1))
            {
                @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_transmitPos = 1;
                transmitPos = @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_AH_transmitPos;
                break;
            }
        }
    }

    // create mode byte
    modeByte = Array_createModeByte (shift, direction, transmitPos, idxSize, recordAddress);

	// Special for SetGet Array: Transmit empty element with elements=panel variable
	if(elements == 0)	// Element deletion
	{
		dataIterator = Array_createHeader_setGet (data, tnle, start_pos, 1, ASG_ID, TAID, modeByte, idxSize);
		data[dataIterator] = 0x00;
		dataIterator++;
	}
	else if(elementsCount == 0 && start_pos == 0xFF)	// Element creation
	{
		dataIterator = Array_createHeader_setGet (data, tnle, start_pos, 1, ASG_ID, TAID, modeByte, idxSize);
		dataIterator = DisplayPresets_addArrayElementData (data, dataIterator, Array_getFirstElementPos(DisplayPresets_Data_Validity_setGet, 0), transmitPos, recordAddress, idxSize);
	}
	else	// 'Normal' setGet
	{
    	dataIterator = Array_createHeader_setGet (data, tnle, start_pos, elementsCount, ASG_ID, TAID, modeByte, idxSize);
		for(i=0; i<elementsCount; i++)
    	{
        	dataIterator = DisplayPresets_addArrayElementData (data, dataIterator, indices[i], transmitPos, recordAddress, idxSize);
    	}
	}

    // Send BAP request
    BAP_createHeader(DISPLAY_PRESETS_FCT_ID, header, dataIterator, DataSetGet_REQ);
    BAP_DLL_Request(DataSetGet_REQ, header, data);
}

void Request_0x1C_DisplayDependency(byte request)
{
    dword header [BAP_HEADER_SIZE];
    byte data[DISPLAY_DEPENDENCY_BAP_DATA_LENGTH];
	
    switch(request)
    {
        case DataGet_REQ_CAN:
            /*** NO FUNCTION WITH BAP DLL (CacheAvailability=true) -> Call via CAN ***/
            CAN_sendGetMessage(DISPLAY_DEPENDENCY_FCT_ID);
            break;

        case DataGet_REQ:
            /*** Cache data request ***/
            BAP_createHeader(DISPLAY_DEPENDENCY_FCT_ID, header, DISPLAY_DEPENDENCY_BAP_DATA_LENGTH, request);
            BAP_DLL_Request(DataGet_REQ, header, data);
            break;

        case DataSetGet_REQ:
			BAP_createHeader(DISPLAY_DEPENDENCY_FCT_ID, header, DISPLAY_DEPENDENCY_BAP_DATA_LENGTH, request);
			createData_0x1C_DisplayDependency(data);
    		BAP_DLL_Request(DataSetGet_REQ, header, data);
            break;

        default:
            break;
    }
}

