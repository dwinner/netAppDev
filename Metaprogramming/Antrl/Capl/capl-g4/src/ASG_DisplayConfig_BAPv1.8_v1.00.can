DisplayPresets_init_CSV_setGet () //Byte & file selection
{
    int i = 0;
    int j = 0;
    int k = 0;

    byte dataBuffer[5000];
    int byteCount = 0;
    char file_complete[5000];

    // byte buffers to store characters
    char buffer_pos							[5];	// Byte: "0x00;"
    char buffer_state						[5];
    char buffer_displayPresetType			[5];
	char buffer_displayPresetInstance		[5];
	char buffer_Display1MainSelection		[11]; 	// 4 Byte: "0x00.00.00.00;"
	char buffer_Display1AdditionalInfo1_1	[11];
	char buffer_Display1AdditionalInfo1_2	[11];
	char buffer_Display1AdditionalInfo2_1	[11];
	char buffer_Display1AdditionalInfo2_2	[11];
	char buffer_Display2MainSelection		[11];
	char buffer_Display2AdditionalInfo1_1	[11];
	char buffer_Display2AdditionalInfo1_2	[11];
	char buffer_Display2AdditionalInfo2_1	[11];
	char buffer_Display2AdditionalInfo2_2	[11];
	char buffer_Display3MainSelection		[11];
	char buffer_Display3AdditionalInfo1_1	[11];
	char buffer_Display3AdditionalInfo1_2	[11];
	char buffer_Display3AdditionalInfo2_1	[11];
	char buffer_Display3AdditionalInfo2_2	[11];
    int buffer_incrementator = 0;

    // differentiate data fields
    enum dataTypeEnumDisplayPreset {posDisplayPreset, state, displayPresetType, displayPresetInstance,
		Display1MainSelection, Display1AdditionalInfo1_1, Display1AdditionalInfo1_2, Display1AdditionalInfo2_1, Display1AdditionalInfo2_2,
		Display2MainSelection, Display2AdditionalInfo1_1, Display2AdditionalInfo1_2, Display2AdditionalInfo2_1, Display2AdditionalInfo2_2,
		Display3MainSelection, Display3AdditionalInfo1_1, Display3AdditionalInfo1_2, Display3AdditionalInfo2_1, Display3AdditionalInfo2_2};
    int dataType = posDisplayPreset;
    int inc_csv_line;
    inc_csv_line = 0;

    // catch if no file was selected
    if(sysGetVariableArrayLength(sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_control_externalFilePath) == 0)
    {
        sysSetVariableString(sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_control_loadReload_result, "Select file first!");
        @sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_control_default_external    = 0;
        return;
    }

    // Get complete path
    sysGetVariableString(sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_control_externalFilePath, file_complete, elCount(file_complete));

    byteCount = File_readCSV (dataBuffer, file_complete);
	
    // clear old data
    DisplayPresets_clearData_setGet();

    /*** Parse data and create new DisplayPresets ***/            
    for(i=0; i<byteCount; i++)
    {
        // nextline in *.csv -> "LF" and "CR" detected
        if( (dataBuffer[i] == 0x0D) && (dataBuffer[i+1] == 0x0A) )
        {
            // Take data if not in first line
            if(inc_csv_line > 0)
            {
                DisplayPresets_Data_Pos_setGet						[atol(buffer_pos)] = atol(buffer_pos);
				DisplayPresets_Data_Validity_setGet					[atol(buffer_pos)] = 1;
				DisplayPresets_Data_State_setGet						[atol(buffer_pos)] = atol(buffer_state);
				DisplayPresets_Data_DisplayPresetType_setGet			[atol(buffer_pos)] = atol(buffer_displayPresetType);
				DisplayPresets_Data_DisplayPresetInstance_setGet		[atol(buffer_pos)] = atol(buffer_displayPresetInstance);
				DisplayPresets_Data_Display1MainSelection_setGet		[atol(buffer_pos)] = atol(buffer_Display1MainSelection);
				DisplayPresets_Data_Display1AdditionalInfo1_1_setGet	[atol(buffer_pos)] = atol(buffer_Display1AdditionalInfo1_1);
				DisplayPresets_Data_Display1AdditionalInfo1_2_setGet	[atol(buffer_pos)] = atol(buffer_Display1AdditionalInfo1_2);
				DisplayPresets_Data_Display1AdditionalInfo2_1_setGet	[atol(buffer_pos)] = atol(buffer_Display1AdditionalInfo2_1);
				DisplayPresets_Data_Display1AdditionalInfo2_2_setGet	[atol(buffer_pos)] = atol(buffer_Display1AdditionalInfo2_2);
				DisplayPresets_Data_Display2MainSelection_setGet		[atol(buffer_pos)] = atol(buffer_Display2MainSelection);
				DisplayPresets_Data_Display2AdditionalInfo1_1_setGet	[atol(buffer_pos)] = atol(buffer_Display2AdditionalInfo1_1);
				DisplayPresets_Data_Display2AdditionalInfo1_2_setGet	[atol(buffer_pos)] = atol(buffer_Display2AdditionalInfo1_2);
				DisplayPresets_Data_Display2AdditionalInfo2_1_setGet	[atol(buffer_pos)] = atol(buffer_Display2AdditionalInfo2_1);
				DisplayPresets_Data_Display2AdditionalInfo2_2_setGet	[atol(buffer_pos)] = atol(buffer_Display2AdditionalInfo2_2);
				DisplayPresets_Data_Display3MainSelection_setGet		[atol(buffer_pos)] = atol(buffer_Display3MainSelection);
				DisplayPresets_Data_Display3AdditionalInfo1_1_setGet	[atol(buffer_pos)] = atol(buffer_Display3AdditionalInfo1_1);
				DisplayPresets_Data_Display3AdditionalInfo1_2_setGet	[atol(buffer_pos)] = atol(buffer_Display3AdditionalInfo1_2);
				DisplayPresets_Data_Display3AdditionalInfo2_1_setGet	[atol(buffer_pos)] = atol(buffer_Display3AdditionalInfo2_1);
				DisplayPresets_Data_Display3AdditionalInfo2_2_setGet	[atol(buffer_pos)] = atol(buffer_Display3AdditionalInfo2_2);
            }

            inc_csv_line++;
            i++;                        // skip CR
            dataType = posDisplayPreset;// starts with pos again
            buffer_incrementator = 0;   // first buffer position

            // clear all buffers
            for(j=0; j<elCount(buffer_pos); j++)
                buffer_pos[j] = 0;
            for(j=0; j<elCount(buffer_state); j++)
                buffer_state[j] = 0;
            for(j=0; j<elCount(buffer_displayPresetType); j++)
                buffer_displayPresetType[j] = 0;
			for(j=0; j<elCount(buffer_displayPresetInstance); j++)
                buffer_displayPresetInstance[j] = 0;
			for(j=0; j<elCount(buffer_Display1MainSelection); j++)
                buffer_Display1MainSelection[j] = 0;
			for(j=0; j<elCount(buffer_Display1AdditionalInfo1_1); j++)
                buffer_Display1AdditionalInfo1_1[j] = 0;
			for(j=0; j<elCount(buffer_Display1AdditionalInfo1_2); j++)
                buffer_Display1AdditionalInfo1_2[j] = 0;
			for(j=0; j<elCount(buffer_Display1AdditionalInfo2_1); j++)
                buffer_Display1AdditionalInfo2_1[j] = 0;
			for(j=0; j<elCount(buffer_Display1AdditionalInfo2_2); j++)
                buffer_Display1AdditionalInfo2_2[j] = 0;
			for(j=0; j<elCount(buffer_Display2MainSelection); j++)
                buffer_Display2MainSelection[j] = 0;
			for(j=0; j<elCount(buffer_Display2AdditionalInfo1_1); j++)
                buffer_Display2AdditionalInfo1_1[j] = 0;
			for(j=0; j<elCount(buffer_Display2AdditionalInfo1_2); j++)
                buffer_Display2AdditionalInfo1_2[j] = 0;
			for(j=0; j<elCount(buffer_Display2AdditionalInfo2_1); j++)
                buffer_Display2AdditionalInfo2_1[j] = 0;
			for(j=0; j<elCount(buffer_Display2AdditionalInfo2_2); j++)
                buffer_Display2AdditionalInfo2_2[j] = 0;
			for(j=0; j<elCount(buffer_Display3MainSelection); j++)
                buffer_Display3MainSelection[j] = 0;
			for(j=0; j<elCount(buffer_Display3AdditionalInfo1_1); j++)
                buffer_Display3AdditionalInfo1_1[j] = 0;
			for(j=0; j<elCount(buffer_Display3AdditionalInfo1_2); j++)
                buffer_Display3AdditionalInfo1_2[j] = 0;
			for(j=0; j<elCount(buffer_Display3AdditionalInfo2_1); j++)
                buffer_Display3AdditionalInfo2_1[j] = 0;
			for(j=0; j<elCount(buffer_Display3AdditionalInfo2_2); j++)
                buffer_Display3AdditionalInfo2_2[j] = 0;
        }
        else
        // Don't work in header line
        if(inc_csv_line > 0)
        {
            // detection of ';'
            if (dataBuffer[i] == 0x3B)
            {
                dataType++;
                buffer_incrementator = 0;
            }
            else // sign to read
            {
                switch(dataType)
                {
                    case posDisplayPreset:
                        buffer_pos[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;

                    case state:
                        buffer_state[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;

                    case displayPresetType:
                        buffer_displayPresetType[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case displayPresetInstance:
                        buffer_displayPresetInstance[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
                    
					case Display1MainSelection:
                        buffer_Display1MainSelection[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display1AdditionalInfo1_1:
                        buffer_Display1AdditionalInfo1_1[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display1AdditionalInfo1_2:
                        buffer_Display1AdditionalInfo1_2[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display1AdditionalInfo2_1:
                        buffer_Display1AdditionalInfo2_1[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display1AdditionalInfo2_2:
                        buffer_Display1AdditionalInfo2_2[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;

					case Display2MainSelection:
                        buffer_Display2MainSelection[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display2AdditionalInfo1_1:
                        buffer_Display2AdditionalInfo1_1[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display2AdditionalInfo1_2:
                        buffer_Display2AdditionalInfo1_2[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display2AdditionalInfo2_1:
                        buffer_Display2AdditionalInfo2_1[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display2AdditionalInfo2_2:
                        buffer_Display2AdditionalInfo2_2[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display3MainSelection:
                        buffer_Display3MainSelection[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display3AdditionalInfo1_1:
                        buffer_Display3AdditionalInfo1_1[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display3AdditionalInfo1_2:
                        buffer_Display3AdditionalInfo1_2[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display3AdditionalInfo2_1:
                        buffer_Display3AdditionalInfo2_1[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
					case Display3AdditionalInfo2_2:
                        buffer_Display3AdditionalInfo2_2[buffer_incrementator] = dataBuffer[i];
                        buffer_incrementator++;
                        break;
						
                    default:    // undefined / error
                        writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR,"Fehler beim Lesen der .csv-Datei! Ungültiger Datentyp: %d", dataType);
                        break;
                }
            }
        }
    }

	sysSetVariableString(sysvar::DisplayConfig_ASG_DisplayPresets::x1B_DisplayPresets_control_loadReload_result, "File successfully loaded!");

	DisplayPresets_Data_startPos_setGet = Array_getFirstElementPos(DisplayPresets_Data_Validity_setGet, 0);
    DisplayPresets_actualize_sysvar_setGet ();

    enableControl("DisplayConfig_ASG_Array_DisplayPresets", "btn_dataSource_external", TRUE);
}

