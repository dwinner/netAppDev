void extractData_0x12_ElementContentSelection(byte indication_data[])
{
	byte transmitPos, recordAddress, idxSize, direction, shift;
	int startEl, elements;
	word wordBuffer;
	int byteCounter, actualPos;
	int i;
	
	// Init helper variable
	transmitPos 	= Array_DecodeModeByte_getTransmitPos(indication_data[2]);
	recordAddress	= Array_DecodeModeByte_getRecordAddress(indication_data[2]);
	idxSize			= Array_DecodeModeByte_getIdxSize(indication_data[2]);
	direction		= Array_DecodeModeByte_getDirection(indication_data[2]);
	shift			= Array_DecodeModeByte_getShift(indication_data[2]);
	
	// Catch invalid RecordAddress
	if    (recordAddress != ELEMENT_CONTENT_SELECTION_RECADDR_RESERVED
		&& recordAddress != ELEMENT_CONTENT_SELECTION_RECADDR_ALL
		&& recordAddress != ELEMENT_CONTENT_SELECTION_RECADDR_EL_CONTENT
		&& recordAddress != ELEMENT_CONTENT_SELECTION_RECADDR_POS)
		return;
	
	//buffer[0] = ASG_ID/TAID;
    //buffer[1] = tnle;
    //buffer[2] = modeByte;  
    if(idxSize == 0)                                                                        // 8 bit addressing
    {
        startEl = indication_data[3];
        elements = indication_data[4];
		byteCounter = 5;
    }
    else                                                                                    // 16 bit addressing
    {
		wordBuffer = indication_data[4];
		wordBuffer = (wordBuffer << 8);
        wordBuffer |= indication_data[3];
		startEl = wordBuffer;

		wordBuffer = indication_data[6];
		wordBuffer = (wordBuffer << 8);
        wordBuffer |= indication_data[5];
		elements = wordBuffer;
		
		byteCounter = 7;
    }
	
	// use Pos parameter to differentiate elements
	if(transmitPos == 1 || recordAddress == ELEMENT_CONTENT_SELECTION_RECADDR_POS)
    {
		for(i=0; i<elements; i++)
		{
			if(idxSize == 0)
			{
				actualPos = indication_data[byteCounter];
				byteCounter++;
			}
			else
			{
				wordBuffer = indication_data[byteCounter + 1];
				wordBuffer = (wordBuffer << 8);
        		wordBuffer |= indication_data[byteCounter];
				actualPos = wordBuffer;
				byteCounter++;
				byteCounter++;
            }
			byteCounter = extractData_0x12_ElementContentSelection_singleElement(indication_data, recordAddress, actualPos, byteCounter);
		}
	}
	// use start, shift, direction and elements parameter
	else
	{
		actualPos = startEl;
		if(direction == 0)
		{
			if (shift == 1)
				actualPos++;
			
			for(i=0; i<elements; i++)
			{
				byteCounter = extractData_0x12_ElementContentSelection_singleElement(indication_data, recordAddress, actualPos, byteCounter);
				actualPos++;
			}
		}
		else
		{
			if (shift == 1)
				actualPos--;
			
			for(i=0; i<elements; i++)
			{
				byteCounter = extractData_0x12_ElementContentSelection_singleElement(indication_data, recordAddress, actualPos, byteCounter);
				actualPos--;
			}
		}
    }
}

int extractData_0x12_ElementContentSelection_singleElement(byte indication_data[], byte recordAddress, word pos, int startByte)
{
	int idx;
	byte byteBuffer;
	idx = startByte;
	
	ElementContentSelection_Data_Validity_get[pos] 				= 1;
	ElementContentSelection_Data_Pos_get[pos] 					= pos;
	
	if(recordAddress == ELEMENT_CONTENT_SELECTION_RECADDR_RESERVED)
	{
		ElementContentSelection_Data_Display_get[pos] 			= 1;
		ElementContentSelection_Data_AdditionalInfo_get[pos] 	= 1;

		ElementContentSelection_Data_Element_get[pos]			= 1;
		
		ElementContentSelection_Data_ElementContent_get[pos] 	= 0;
    }
	
    // Display, AdditionalInfo, Element - if transmission is enabled via recordAddress
	if(recordAddress == ELEMENT_CONTENT_SELECTION_RECADDR_ALL)
	{
		byteBuffer = indication_data[idx];
		ElementContentSelection_Data_Display_get[pos] 			= (byteBuffer & 0xF0) >> 4;
		ElementContentSelection_Data_AdditionalInfo_get[pos] 	= (byteBuffer & 0x0F) >> 0;
		idx++;

		ElementContentSelection_Data_Element_get[pos]			= indication_data[idx];
		idx++;
		
		ElementContentSelection_Data_ElementContent_get[pos] 	= indication_data[idx];
		idx++;
    }
	
    // ElementContent - if transmission is enabled via recordAddress
	if(recordAddress == ELEMENT_CONTENT_SELECTION_RECADDR_EL_CONTENT)
	{
		ElementContentSelection_Data_Display_get[pos] 			= 1;
		ElementContentSelection_Data_AdditionalInfo_get[pos] 	= 1;

		ElementContentSelection_Data_Element_get[pos]			= 1;
		
		ElementContentSelection_Data_ElementContent_get[pos] 	= indication_data[idx];
		idx++;
    }
	
	ElementContentSelection_actualize_sysvar_get();
	
	return idx;
}

