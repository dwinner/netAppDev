void Indication_0x01_GetAll(dword indication_header [], byte indication_data[], int datalength)
{
    switch (indication_header[2])
	{
		case Data_IND:
			setTimer(startupTimer, 1500);
            break;

        case Error_IND:
            writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "DisplayConfig_ASG: Error-Indication in Indication_0x01_GetAll: Error 0x%x", indication_header[5]);
		    break;

        default:
			writelineEx(gBAP_Trace, CAPL_WRITE_LINE_INFORMATION, "DisplayConfig_ASG: Unhandled indication 0x%x in Indication_0x01_GetAll.", indication_header[2]);
		    break;
	} 
}

void Indication_0x02_BAP_Config(dword indication_header [], byte indication_data[], int datalength)
{
    switch (indication_header[2])
	{
		case Data_IND:
        case Reset_IND:
			extractData_0x02_BAP_Config(indication_data);
			
			if(@sysvar::DisplayConfig_General::Smart)
        		Request_0x03_FunctionList(DataGet_REQ);	
            break;
        
		case Error_IND:
            writelineEx(gError_Trace, CAPL_WRITE_LINE_ERROR, "DisplayConfig_ASG: Error-Indication in Indication_0x02_BAP_Config: Error 0x%x", indication_header[5]);
		    break;
			
        default:
			writelineEx(gBAP_Trace, CAPL_WRITE_LINE_INFORMATION, "DisplayConfig_ASG: Unhandled indication 0x%x in Indication_0x02_BAP_Config.", indication_header[2]);
		    break;
	}
    
    controls_setFunctionActive(FUNCTION_LIST_FCT_ID, TRUE);
}

void extractData_0x02_BAP_Config(byte indication_data[])
{
	@sysvar::DisplayConfig_ASG::x02_BAPConfig_BAP_major 	= indication_data[0];
	@sysvar::DisplayConfig_ASG::x02_BAPConfig_BAP_minor 	= indication_data[1];
	@sysvar::DisplayConfig_ASG::x02_BAPConfig_LSG_Class 	= indication_data[2];
	@sysvar::DisplayConfig_ASG::x02_BAPConfig_LSG_SubClass 	= indication_data[3];
	@sysvar::DisplayConfig_ASG::x02_BAPConfig_DF_VerMajor 	= indication_data[4];
	@sysvar::DisplayConfig_ASG::x02_BAPConfig_DF_VerMinor 	= indication_data[5];
}

void extractData_0x03_FunctionList(byte indication_data[])
{
	// Byte [0]
	@sysvar::DisplayConfig_ASG::x03_FctList_0x01_GetAll            			= (indication_data[0] >> 6) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x02_BAPConfig        			= (indication_data[0] >> 5) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x03_FctList           			= (indication_data[0] >> 4) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x04_HeartBeat         			= (indication_data[0] >> 3) & 0x01;
	
	// Byte [1]
	@sysvar::DisplayConfig_ASG::x03_FctList_0x0D_FSG_Control      			= (indication_data[1] >> 2) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x0E_FSG_Setup        			= (indication_data[1] >> 1) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x0F_FSG_OperationState			= (indication_data[1] >> 0) & 0x01;
	
	// Byte [2]
	@sysvar::DisplayConfig_ASG::x03_FctList_0x10_Brightness					= (indication_data[2] >> 7) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x11_Volume						= (indication_data[2] >> 6) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x12_ElementContentSelection	= (indication_data[2] >> 5) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x13_Display1Setup				= (indication_data[2] >> 4) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x14_Display1MainSelection		= (indication_data[2] >> 3) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x15_Display2Setup				= (indication_data[2] >> 2) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x16_Display2MainSelection		= (indication_data[2] >> 1) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x17_Display3Setup				= (indication_data[2] >> 0) & 0x01;
		
	// Byte [3]	
	@sysvar::DisplayConfig_ASG::x03_FctList_0x18_Display3MainSelection		= (indication_data[3] >> 7) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x19_AdditionalInstrument		= (indication_data[3] >> 6) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x1A_AdditionalInstrument2		= (indication_data[3] >> 5) & 0x01;
	@sysvar::DisplayConfig_ASG::x03_FctList_0x1B_DisplayPresets				= (indication_data[3] >> 4) & 0x01;
