on envVar mci_NavigationSD_ManeuverDescriptor_Maneuver1_Sidestreets
{
  int i=0,j=0;
  char ctmp[120];
  byte btmp[120];
  int len;
  
  // String auslesen
  getvalue(this, ctmp);
  
  //Lnge auslesen
  len = getvaluesize(this);
 
  // String auswerten
  if (len !=0)
  {
    for (i=0;i<=len;i++)
    {
      // 0...9
      if (ctmp[i] >= 0x30 && ctmp[i] <= 0x39) 
      {
        if(ctmp[i+1] == 0x30)
          {
            btmp[j++] = (ctmp[i]-48)*16;
          }
          
        else
          {
            btmp[j++] = (ctmp[i]-48)*16 + (ctmp[i+1]-48);
          }
          
        i++;
      }
      
      // A...F
      if (ctmp[i] >= 0x41 && ctmp[i] <= 0x46)
      {
        if(ctmp[i+1] == 0x30)
          {
            btmp[j++] = ((ctmp[i]-55)*16);
          }
          
        else
          {
            btmp[j++] = ((ctmp[i]-55)*16) + (ctmp[i+1]-48);
          }
          
        i++;
         
      }
      
      // a...f
      if (ctmp[i] >= 0x61 && ctmp[i] <= 0x66)
      {
        if(ctmp[i+1] == 0x30)
          {
            btmp[j++] = ((ctmp[i]-87)*16);
          }
          
        else
          {
            btmp[j++] = ((ctmp[i]-87)*16) + (ctmp[i+1]-87);
          }
          
        i++;
      }
    }
  }
  putvalue(P_FSG_50_0_0_23_1_Sidestreets, btmp, (len-1)/2);
  j=0;
}

