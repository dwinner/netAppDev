on envVar mci_OnlineFunctions_TrafficLightOnlineInfo_Sidestreets
{
  int i=0,j=0;
  char ctmp[120];
  byte btmp[120];
  int len;
  byte high, low;
  
  // String auslesen
  getvalue(this, ctmp);
  
  //Lnge auslesen
  len = getvaluesize(this);
 
  // String auswerten
  if (len !=0)
  {
   for (i=0;i<=len;i++)
    {
      // high nibble
      if (ctmp[i] >= 0x30 && ctmp[i] <= 0x39) // 0...9
      {
        high = (ctmp[i]-0x30);
      }
      if (ctmp[i] >= 0x41 && ctmp[i] <= 0x46) // A...F
      {
        high = (ctmp[i]-0x37);
      }
      if (ctmp[i] >= 0x61 && ctmp[i] <= 0x66) // a...f
      {
        high = (ctmp[i]-0x57);
      }
      // low nibble
      if (ctmp[i+1] >= 0x30 && ctmp[i+1] <= 0x39) // 0...9
      {
        low = (ctmp[i+1]-0x30) & 0x0F;
      }
      if (ctmp[i+1] >= 0x41 && ctmp[i+1] <= 0x46) // A...F
      {
        low = (ctmp[i+1]-0x37) & 0x0F;
      }
      if (ctmp[i+1] >= 0x61 && ctmp[i+1] <= 0x66) // a...f
      {
        low = (ctmp[i+1]-0x57) & 0x0F;
      }
      
      //writelineex(4,0,"high: %x / low: %x",high,low);
      btmp[j++] = (high * 16) + low;
        
      i++;
    }
  }
  putvalue(P_FSG_42_0_0_16_3, btmp, (len-1)/2);
  j=0;
}

