Temperature_temperatures_Notification()
{
  word data_length;
  word data_length_element_sum;
  word data_length_element[10];
  byte wiretype;
  byte data_id;
  word start_pos;
  
  payload_rx_counter = 0;
  data_length_element_sum = 0;
  Temperature_temperatures_data_length_element_num = 0;
  Temperature_temperatures_available_reset();
  
  // Gesamtlaenge
  data_length = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
  
  // pruefen ob noch Elemente vorhanden sind
  while (data_length_element_sum < data_length)
  {
    // Laenge Element
    data_length_element[Temperature_temperatures_data_length_element_num] = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
                                                                          +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
                                                                          +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
                                                                          +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
   
    start_pos = payload_rx_counter;
    
    while (payload_rx_counter < data_length_element[Temperature_temperatures_data_length_element_num] + start_pos)
    {
      // wiretype & data_id pruefen
      wiretype = (someip_payload_buffer[payload_rx_counter++]>>4) & 0x0F;
      data_id = someip_payload_buffer[payload_rx_counter++];
      
      switch (data_id)
      {
        case 0:  Temperature_Notification_temperatures_type(); break;
        case 1:  Temperature_Notification_temperatures_currentValue(); break;
        case 2:  Temperature_Notification_temperatures_restrictionReason(); break;
        case 3:  Temperature_Notification_temperatures_unit(); break;
        case 4:  Temperature_Notification_temperatures_minValue(); break;
        case 5:  Temperature_Notification_temperatures_maxValue(); break;
        case 6:  Temperature_Notification_temperatures_resolution(); break;
        case 7:  Temperature_Notification_temperatures_isValueInvalid(); break;
      }
    }
    data_length_element_sum = data_length_element_sum + data_length_element[Temperature_temperatures_data_length_element_num] + 4;
    
    if (data_length_element_sum < data_length)
      Temperature_temperatures_data_length_element_num++;
  }
  @Temperature_SetGet_temperatures_Element_TotalNum = Temperature_temperatures_data_length_element_num+1;
