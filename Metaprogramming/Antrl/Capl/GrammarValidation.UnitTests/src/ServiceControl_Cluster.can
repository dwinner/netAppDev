DisplaySpeed_SetGet_speeds()
{
  word payload_counter = 0;
  word start_pos       = 0;
  word element_length  = 0;
  
  // Length Indicator Struct - Platzhalter
  payload_counter = 0+4;
  start_pos = 0;
  
  for (j=0; j<@DisplaySpeed_SetGet_speeds_Element_TotalNum; j++)
  {
    DisplaySpeed_array_speeds_available_functions[j] = DisplaySpeed_array_speeds_type_available[j]
                                                            + DisplaySpeed_array_speeds_currentValue_available[j]
                                                            + DisplaySpeed_array_speeds_restrictionReason_available[j]
                                                            + DisplaySpeed_array_speeds_unit_available[j]
																	                          + DisplaySpeed_array_speeds_isDisplayActivated_available[j]
