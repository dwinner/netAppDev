}


DiagnosticNotification_notifications_Notification()
{
  word data_length;
  word data_length_element_sum;
  word data_length_element[10];
  byte wiretype;
  byte data_id;
  word start_pos;
  
  payload_rx_counter = 0;
  data_length_element_sum = 0;
  DiagnosticNotification_notifications_data_length_element_num = 0;
  DiagnosticNotification_notifications_available_reset();
  
  // Gesamtlaenge
  data_length = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
  
  // pruefen ob noch Elemente vorhanden sind
  while (data_length_element_sum < data_length)
  {
    // Laenge Element
    data_length_element[DiagnosticNotification_notifications_data_length_element_num] = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
                                                                                      +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
                                                                                      +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
                                                                                      +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
   
    start_pos = payload_rx_counter;
    
    while (payload_rx_counter < data_length_element[DiagnosticNotification_notifications_data_length_element_num] + start_pos)
    {
      // wiretype & data_id pruefen
      wiretype = (someip_payload_buffer[payload_rx_counter++]>>4) & 0x0F;
      data_id = someip_payload_buffer[payload_rx_counter++];
      
      switch (data_id)
      {
        case 0:  DiagnosticNotification_Notification_notifications_listId(); break;
        case 1:  DiagnosticNotification_Notification_notifications_notificationIds(); break;
      }
    }
    data_length_element_sum = data_length_element_sum + data_length_element[DiagnosticNotification_notifications_data_length_element_num] + 4;
    
    if (data_length_element_sum < data_length)
      DiagnosticNotification_notifications_data_length_element_num++;
  }
  @DiagnosticNotification_SetGet_notifications_Element_TotalNum = DiagnosticNotification_notifications_data_length_element_num+1;
}

DiagnosticNotification_notifications_available_reset()
{
  for (k=0; k<10; k++)
  {
    DiagnosticNotification_array_notifications_listId_available[k]                              = 0;
    DiagnosticNotification_array_notifications_notificationIds_available[k]                      = 0;
  }
}


DiagnosticNotification_Notification_notifications_listId()
{
  DiagnosticNotification_array_notifications_listId[DiagnosticNotification_notifications_data_length_element_num] = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
                                                                                                                  +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
                                                                                                                  +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
                                                                                                                  +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
  
  DiagnosticNotification_array_notifications_listId_available[DiagnosticNotification_notifications_data_length_element_num] = 1;
  DiagnosticNotification_SetGet_notifications_update();
}
DiagnosticNotification_Notification_notifications_notificationIds()
{
  byte length;
  
  length = someip_payload_buffer[payload_rx_counter++];
  
  for (k=0; k<length; k++)
  {
    DiagnosticNotification_array_notifications_notificationIds[DiagnosticNotification_notifications_data_length_element_num][k] = someip_payload_buffer[payload_rx_counter++];
  }
  
  DiagnosticNotification_array_notifications_notificationIds_available[DiagnosticNotification_notifications_data_length_element_num] = 1;
  DiagnosticNotification_SetGet_notifications_update();
}












