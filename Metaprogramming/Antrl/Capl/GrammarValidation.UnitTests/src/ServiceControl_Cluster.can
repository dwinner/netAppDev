DisplaySpeed_speeds_Notification()
{
  word data_length;
  word data_length_element_sum;
  word data_length_element[10];
  byte wiretype;
  byte data_id;
  word start_pos;
  
  payload_rx_counter = 0;
  data_length_element_sum = 0;
  DisplaySpeed_speeds_data_length_element_num = 0;
  DisplaySpeed_speeds_available_reset();
  
  // Gesamtlaenge
  data_length = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
  
  // pruefen ob noch Elemente vorhanden sind
  while (data_length_element_sum < data_length)
  {
    // Laenge Element
    data_length_element[DisplaySpeed_speeds_data_length_element_num] = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
                                                                     +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
                                                                     +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
                                                                     +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
   
    start_pos = payload_rx_counter;
    
    while (payload_rx_counter < data_length_element[DisplaySpeed_speeds_data_length_element_num] + start_pos)
    {
      // wiretype & data_id pruefen
      wiretype = (someip_payload_buffer[payload_rx_counter++]>>4) & 0x0F;
      data_id = someip_payload_buffer[payload_rx_counter++];
      
      switch (data_id)
      {
        case 0:  DisplaySpeed_Notification_speeds_type(); break;
        case 1:  DisplaySpeed_Notification_speeds_currentValue(); break;
        case 2:  DisplaySpeed_Notification_speeds_restrictionReason(); break;
        case 3:  DisplaySpeed_Notification_speeds_unit(); break;
        case 4:  DisplaySpeed_Notification_speeds_isDisplayActivated(); break;
        case 5:  DisplaySpeed_Notification_speeds_maxValue(); break;
        case 6:  DisplaySpeed_Notification_speeds_minValue(); break;
        case 7:  DisplaySpeed_Notification_speeds_resolution(); break;
      }
    }
    data_length_element_sum = data_length_element_sum + data_length_element[DisplaySpeed_speeds_data_length_element_num] + 4;
    
    if (data_length_element_sum < data_length)
      DisplaySpeed_speeds_data_length_element_num++;
  }
  @DisplaySpeed_SetGet_speeds_Element_TotalNum = DisplaySpeed_speeds_data_length_element_num+1;
}

