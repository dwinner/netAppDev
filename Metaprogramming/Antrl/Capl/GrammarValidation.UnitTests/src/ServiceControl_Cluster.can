    if (DrivingStatistics_array_durations_array_element_length[j] == 0) payload_counter = payload_counter+4;
  }
  
  // Struct Length
  DrivingStatistics_array_durations_struct_element_length = 4*@DrivingStatistics_SetGet_durations_Element_TotalNum;

  for (k=0;k<@DrivingStatistics_SetGet_durations_Element_TotalNum;k++)
    DrivingStatistics_array_durations_struct_element_length = DrivingStatistics_array_durations_struct_element_length + DrivingStatistics_array_durations_array_element_length[k];
  
  DrivingStatistics_payload_buffer_durations[0] = DrivingStatistics_array_durations_struct_element_length>>24 & 0xFF;
  DrivingStatistics_payload_buffer_durations[1] = DrivingStatistics_array_durations_struct_element_length>>16 & 0xFF;
  DrivingStatistics_payload_buffer_durations[2] = DrivingStatistics_array_durations_struct_element_length>> 8 & 0xFF;
