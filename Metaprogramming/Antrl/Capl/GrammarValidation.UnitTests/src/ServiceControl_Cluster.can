Distance_Notification_distances()
{
  word payload_counter = 0;
  word start_pos       = 0;
  word element_length  = 0;
  
  // Length Indicator Struct - Platzhalter
  payload_counter = 0+4;
  start_pos = 0;
  
  for (j=0; j<@Distance_Notification_distances_Element_TotalNum; j++)
  {
    Distance_array_distances_available_functions[j] = Distance_array_distances_distanceName_available[j]
                                                    + Distance_array_distances_currentValue_available[j]
                                                    + Distance_array_distances_restrictionReason_available[j]
                                                    + Distance_array_distances_unit_available[j];
    
    // Length Indicator Element - Platzhalter
    if (Distance_array_distances_available_functions[j] > 0)
    {
      start_pos = payload_counter;
      payload_counter = payload_counter + 4;
    }
    else 
      start_pos = payload_counter;
    
    
    // 0 distanceName (enum32)
    if (Distance_array_distances_distanceName_available[j] == 1)
    {
      tag_dataID = 0;
      Distance_payload_buffer_distances[payload_counter++] = 2 * 16 /* Wiretype = 2 =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Distance_payload_buffer_distances[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_distanceName[j]>>24 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_distanceName[j]>>16 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_distanceName[j]>> 8 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_distanceName[j]     & 0xFF;
    }
    
    
    // 1 currentValue (uint64)
    if (Distance_array_distances_currentValue_available[j] == 1)
    {
      tag_dataID = 1;
      Distance_payload_buffer_distances[payload_counter++] = 3 * 16 /* Wiretype = 3  =^ 64 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Distance_payload_buffer_distances[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      temp_64bit_low  = Distance_array_distances_currentValue[j]     & 0x00000000FFFFFFFF;
      temp_64bit_high = Distance_array_distances_currentValue[j]>>32 & 0x00000000FFFFFFFF;
      
      Distance_payload_buffer_distances[payload_counter++] = temp_64bit_high>>24 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = temp_64bit_high>>16 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = temp_64bit_high>> 8 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = temp_64bit_high     & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = temp_64bit_low>>24  & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = temp_64bit_low>>16  & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = temp_64bit_low>> 8  & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = temp_64bit_low      & 0xFF;
    }
    
    
    // 2 restrictionReason (enum32)
    if (Distance_array_distances_restrictionReason_available[j] == 1)
    {
      tag_dataID = 2;
      Distance_payload_buffer_distances[payload_counter++] = 2 * 16 /* Wiretype = 2 =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Distance_payload_buffer_distances[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_restrictionReason[j]>>24 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_restrictionReason[j]>>16 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_restrictionReason[j]>> 8 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_restrictionReason[j]     & 0xFF;
    }
    
    
    // 3 unit (enum32)
    if (Distance_array_distances_unit_available[j] == 1)
    {
      tag_dataID = 3;
      Distance_payload_buffer_distances[payload_counter++] = 2 * 16 /* Wiretype = 2 =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Distance_payload_buffer_distances[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_unit[j]>>24 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_unit[j]>>16 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_unit[j]>> 8 & 0xFF;
      Distance_payload_buffer_distances[payload_counter++] = Distance_array_distances_unit[j]     & 0xFF;
    }
    
    //Element Length
    element_length = payload_counter-start_pos;
    if (element_length >0) Distance_array_distances_array_element_length[j] = element_length - 4;
    else                   Distance_array_distances_array_element_length[j] = element_length;
    
    Distance_payload_buffer_distances[start_pos++] = Distance_array_distances_array_element_length[j]>>24 & 0xFF;
    Distance_payload_buffer_distances[start_pos++] = Distance_array_distances_array_element_length[j]>>16 & 0xFF;
    Distance_payload_buffer_distances[start_pos++] = Distance_array_distances_array_element_length[j]>> 8 & 0xFF;
    Distance_payload_buffer_distances[start_pos++] = Distance_array_distances_array_element_length[j]     & 0xFF;
    
    if (Distance_array_distances_array_element_length[j] == 0) payload_counter = payload_counter+4;
  }
  
  // Struct Length
  Distance_array_distances_struct_element_length = 4*@Distance_Notification_distances_Element_TotalNum;

  for (k=0;k<@Distance_Notification_distances_Element_TotalNum;k++)
   Distance_array_distances_struct_element_length = Distance_array_distances_struct_element_length + Distance_array_distances_array_element_length[k];
  
  Distance_payload_buffer_distances[0] = Distance_array_distances_struct_element_length>>24 & 0xFF;
  Distance_payload_buffer_distances[1] = Distance_array_distances_struct_element_length>>16 & 0xFF;
  Distance_payload_buffer_distances[2] = Distance_array_distances_struct_element_length>> 8 & 0xFF;
  Distance_payload_buffer_distances[3] = Distance_array_distances_struct_element_length     & 0xFF;


  Distance_payload_length_distances = Distance_array_distances_struct_element_length+4;
  putvalue(Distance_Notification_distances_Payload, Distance_payload_buffer_distances, Distance_payload_length_distances);
}

on envVar Distance_Notification_distances_Element_TotalNum
{
  if (@this == 0) @this =  1;
  if (@this > 10) @this = 10;
  
  Distance_Notification_distances();
