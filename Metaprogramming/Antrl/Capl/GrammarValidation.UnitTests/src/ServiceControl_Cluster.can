  @Distance_Notification_distances_distanceName      = Distance_array_distances_distanceName[@Distance_Notification_distances_Element];
  @Distance_Notification_distances_currentValue      = Distance_array_distances_currentValue[@Distance_Notification_distances_Element];
  @Distance_Notification_distances_restrictionReason = Distance_array_distances_restrictionReason[@Distance_Notification_distances_Element];
  @Distance_Notification_distances_unit              = Distance_array_distances_unit[@Distance_Notification_distances_Element];
  
  @Distance_Notification_distances_distanceName_available      = Distance_array_distances_distanceName_available[@Distance_Notification_distances_Element];
  @Distance_Notification_distances_currentValue_available      = Distance_array_distances_currentValue_available[@Distance_Notification_distances_Element];
  @Distance_Notification_distances_restrictionReason_available = Distance_array_distances_restrictionReason_available[@Distance_Notification_distances_Element];
  @Distance_Notification_distances_unit_available              = Distance_array_distances_unit_available[@Distance_Notification_distances_Element];
}






