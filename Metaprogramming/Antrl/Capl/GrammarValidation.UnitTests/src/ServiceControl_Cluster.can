    servicediscovery_sub_tx_entries_array_instance_id[j]       = 1;
    servicediscovery_sub_tx_entries_array_major_version[j]     = 3;
    servicediscovery_sub_tx_entries_array_ttl[j]               = 0xFFFFFF * Status;
    servicediscovery_sub_tx_entries_array_counter[j]           = 0;
    servicediscovery_sub_tx_entries_array_eventgroup_id[j]     = Eventgroup_ID;
    
    j++;
  }
  
  // DrivingStatistics_durations
  if (Service_ID == 0x6035 && Eventgroup_ID == 0xC030)
  {
    servicediscovery_sub_tx_entries_array_type[j]              = 6;
    servicediscovery_sub_tx_entries_array_index_1st_options[j] = 0;
    servicediscovery_sub_tx_entries_array_index_2nd_options[j] = 0;
    servicediscovery_sub_tx_entries_array_nr_of_opt_1[j]       = 1;
    servicediscovery_sub_tx_entries_array_nr_of_opt_2[j]       = 0;
    servicediscovery_sub_tx_entries_array_service_id[j]        = Service_ID;
    servicediscovery_sub_tx_entries_array_instance_id[j]       = 1;
    servicediscovery_sub_tx_entries_array_major_version[j]     = 3;
    servicediscovery_sub_tx_entries_array_ttl[j]               = 0xFFFFFF * Status;
    servicediscovery_sub_tx_entries_array_counter[j]           = 0;
    servicediscovery_sub_tx_entries_array_eventgroup_id[j]     = Eventgroup_ID;
    
    j++;
  }
  
  // DiagnosticNotification_notifications
  if (Service_ID == 0x6046 && Eventgroup_ID == 0xC010)
  {
    servicediscovery_sub_tx_entries_array_type[j]              = 6;
    servicediscovery_sub_tx_entries_array_index_1st_options[j] = 0;
    servicediscovery_sub_tx_entries_array_index_2nd_options[j] = 0;
    servicediscovery_sub_tx_entries_array_nr_of_opt_1[j]       = 1;
    servicediscovery_sub_tx_entries_array_nr_of_opt_2[j]       = 0;
    servicediscovery_sub_tx_entries_array_service_id[j]        = 0x6046;
    servicediscovery_sub_tx_entries_array_instance_id[j]       = 1;
    servicediscovery_sub_tx_entries_array_major_version[j]     = 1;
    servicediscovery_sub_tx_entries_array_ttl[j]               = 0xFFFFFF * Status;
    servicediscovery_sub_tx_entries_array_counter[j]           = 0;
    servicediscovery_sub_tx_entries_array_eventgroup_id[j]     = 0xC010;
    
    j++;
  }
  
  for (j=0; j < number_of_subscribed_services_by_ICAS1Sys02; j++)
  {
    // SOME/IP Packet - Entries Array - Type [8 bit]
    buffer[i++] = servicediscovery_sub_tx_entries_array_type[j];
    
    // SOME/IP Packet - Entries Array - Index 1st options [8 bit]
    buffer[i++] = servicediscovery_sub_tx_entries_array_index_1st_options[j];
    
    // SOME/IP Packet - Entries Array - Index 2nd options [8 bit]
    buffer[i++] = servicediscovery_sub_tx_entries_array_index_2nd_options[j];
    
    // SOME/IP Packet - Entries Array - # of opt 1 [4 bit] / # of opt 2 [4 bit]
    buffer[i++] = (((servicediscovery_sub_tx_entries_array_nr_of_opt_1[j]*16)) + (servicediscovery_sub_tx_entries_array_nr_of_opt_2[j]));
    
    // SOME/IP Packet - Entries Array - Service ID [16 bit]
    buffer[i++] = servicediscovery_sub_tx_entries_array_service_id[j]>> 8 & 0xFF;
    buffer[i++] = servicediscovery_sub_tx_entries_array_service_id[j]     & 0xFF;

    // SOME/IP Packet - Entries Array - Instance ID [16 bit]
    buffer[i++] = servicediscovery_sub_tx_entries_array_instance_id[j]>> 8 & 0xFF;
    buffer[i++] = servicediscovery_sub_tx_entries_array_instance_id[j]     & 0xFF;
    
    // SOME/IP Packet - Entries Array - Major Version [8 bit]
    buffer[i++] = servicediscovery_sub_tx_entries_array_major_version[j];
    
    // SOME/IP Packet - Entries Array - TTL [24 bit]
    buffer[i++] = servicediscovery_sub_tx_entries_array_ttl[j]>>16 & 0xFF;
    buffer[i++] = servicediscovery_sub_tx_entries_array_ttl[j]>> 8 & 0xFF;
    buffer[i++] = servicediscovery_sub_tx_entries_array_ttl[j]     & 0xFF;
    
    // SOME/IP Packet - Entries Array - Reserved [12 bit] + Counter [4 bit]
    buffer[i++] = 0x00;
    buffer[i++] = servicediscovery_sub_tx_entries_array_counter[j] & 0x0F;
    
    // SOME/IP Packet - Entries Array - Eventgroup ID
    buffer[i++] = servicediscovery_sub_tx_entries_array_eventgroup_id[j]>> 8 & 0xFF;
    buffer[i++] = servicediscovery_sub_tx_entries_array_eventgroup_id[j]     & 0xFF;  
  }
  
  
  //SOME/IP Packet - Options Array - Length [32 bit]
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Length>>24 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Length>>16 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Length>> 8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Length     & 0xFF;

  //SOME/IP Packet - Options Array - Option1 Length [16 bit]
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_Length>> 8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_Length     & 0xFF;
  
  //SOME/IP Packet - Options Array - Option1 Type [8 bit]
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_Type & 0xFF;
  
  //SOME/IP Packet - Options Array - Reserved [8 bit]
  buffer[i++] = 0x00;
  
  //SOME/IP Packet - Options Array - Option1 IPv6 Address
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_01>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_01    & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_02>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_02    & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_03>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_03    & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_04>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_04    & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_05>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_05    & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_06>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_06    & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_07>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_07    & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_08>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_IPv6_08    & 0xFF;
  
  //SOME/IP Packet - Options Array - Reserved [8 bit]
  buffer[i++] = 0x00;
  
  //SOME/IP Packet - Options Array - Option1 L4-Proto [8 bit]
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_L4_Proto;
  
  //SOME/IP Packet - Options Array - Option1 Port Number [16 bit]
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_Port_Number>> 8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Sub_ICAS1Sys02_Options_Array_Option1_Port_Number     & 0xFF;
  
  // Rohdaten in UDP Paket schreiben
  EthPaket.udp.SetData( 0, buffer, i );

  // calculate UDP and IPv6 checksum and send Ethernet packet
  EthPaket.CompletePacket();
  if (@Env_GlobalAppl == 1) output( EthPaket );  
}








