DrivingStatistics_SetGet_speeds_update()
{
  @DrivingStatistics_SetGet_LengthIndicator_Element_speeds = DrivingStatistics_array_speeds_array_element_length[@DrivingStatistics_SetGet_speeds_Element];
  @DrivingStatistics_SetGet_LengthIndicator_Struct_speeds  = DrivingStatistics_array_speeds_struct_element_length;
  @DrivingStatistics_SetGet_Length_speeds                  = DrivingStatistics_payload_length_speeds+8;
  
  @DrivingStatistics_SetGet_speeds_memoryLevel  = DrivingStatistics_array_speeds_memoryLevel[@DrivingStatistics_SetGet_speeds_Element];
  @DrivingStatistics_SetGet_speeds_currentValue = DrivingStatistics_array_speeds_currentValue[@DrivingStatistics_SetGet_speeds_Element];
