UnitMaster_SetGet_units()
{
  word payload_counter = 0;
  word start_pos       = 0;
  word element_length  = 0;
  
  // Length Indicator Struct - Platzhalter
  payload_counter = 0+4;
  start_pos = 0;
  
  for (j=0; j<@UnitMaster_SetGet_units_Element_TotalNum; j++)
  {
    UnitMaster_array_units_available_functions[j] = UnitMaster_array_units_id_available[j]
                                                  + UnitMaster_array_units_name_available[j]
                                                  + UnitMaster_array_units_dateFormat_available[j]
                                                  + UnitMaster_array_units_dateFormatConfiguration_available[j]
                                                  + UnitMaster_array_units_distanceUnit_available[j]
                                                  + UnitMaster_array_units_distanceUnitConfiguration_available[j]
                                                  + UnitMaster_array_units_pressureUnit_available[j]
                                                  + UnitMaster_array_units_pressureUnitConfiguration_available[j]
                                                  + UnitMaster_array_units_restrictionReason_available[j]
                                                  + UnitMaster_array_units_speedUnitTachometer_available[j]
                                                  + UnitMaster_array_units_speedUnit_available[j]
                                                  + UnitMaster_array_units_speedUnitConfiguration_available[j]
                                                  + UnitMaster_array_units_temperatureUnit_available[j]
                                                  + UnitMaster_array_units_temperatureUnitConfiguration_available[j]
                                                  + UnitMaster_array_units_timeFormat_available[j]
                                                  + UnitMaster_array_units_timeFormatConfiguration_available[j]
                                                  + UnitMaster_array_units_volumeUnit_available[j]
                                                  + UnitMaster_array_units_volumeUnitConfiguration_available[j]
                                                  + UnitMaster_array_units_consumptionUnit_available[j]
                                                  + UnitMaster_array_units_consumptionUnitConfiguration_available[j]
                                                  + UnitMaster_array_units_consumptionUnitElectr_available[j]
                                                  + UnitMaster_array_units_consumptionUnitElectrConfiguration_available[j]
                                                  + UnitMaster_array_units_consumptionUnitGas_available[j]
                                                  + UnitMaster_array_units_consumptionUnitGasConfiguration_available[j]
                                                  + UnitMaster_array_units_massUnit_available[j]
                                                  + UnitMaster_array_units_massUnitConfiguration_available[j];
    
    // Length Indicator Element - Platzhalter
    if (UnitMaster_array_units_available_functions[j] > 0)
    {
      start_pos = payload_counter;
      payload_counter = payload_counter + 4;
    }
    else 
      start_pos = payload_counter;
    
    
    // 0 id (uuid 16byte)
    if (UnitMaster_array_units_id_available[j] == 1)
    {
      tag_dataID = 0;
      UnitMaster_payload_buffer_units[payload_counter++] = 5 * 16 /* Wiretype = 5 =^ Array mit 1 Byte Lnge */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      UnitMaster_payload_buffer_units[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      UnitMaster_payload_buffer_units[payload_counter++] = 16; // Array Length
      
      for (k=0; k<16; k++)
        UnitMaster_payload_buffer_units[payload_counter++] = UnitMaster_array_units_id[j][k] & 0xFF;
    }
    
    
    // 1 name (string)
    if (UnitMaster_array_units_name_available[j] == 1)
    {
      tag_dataID = 1;
      UnitMaster_payload_buffer_units[payload_counter++] = 5 * 16 /* Wiretype = 5 =^ Array mit 1 Byte Lnge */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      UnitMaster_payload_buffer_units[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      UnitMaster_payload_buffer_units[payload_counter++] = strlen(UnitMaster_array_units_name[j]);  
      
      if (strlen(UnitMaster_array_units_name[j]) > 1)
      {
        for (k=0; k<strlen(UnitMaster_array_units_name[j]); k++)
          UnitMaster_payload_buffer_units[payload_counter++] = UnitMaster_array_units_name[j][k];
      }
    }
    
    
    // 2 dateFormat (enum32)
    if (UnitMaster_array_units_dateFormat_available[j] == 1)
    {
      tag_dataID = 2;
      UnitMaster_payload_buffer_units[payload_counter++] = 2 * 16 /* Wiretype = 2 =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      UnitMaster_payload_buffer_units[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      UnitMaster_payload_buffer_units[payload_counter++] = UnitMaster_array_units_dateFormat[j]>>24 & 0xFF;
