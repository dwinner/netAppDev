on envVar Distance_Notification_notifications_value_available
{
  Distance_array_notifications_value_available[@Distance_Notification_notifications_Element] = @this;
  Distance_Notification_notifications();
}
on envVar Distance_Notification_notifications_Payload
{
  Distance_Notification_notifications_update();
}


Distance_Notification_notifications()
{
  word payload_counter = 0;
  word start_pos       = 0;
  word element_length  = 0;
  
  // Length Indicator Struct - Platzhalter
  payload_counter = 0+4;
  start_pos = 0;
  
  for (j=0; j<@Distance_Notification_notifications_Element_TotalNum; j++)
  {
    Distance_array_notifications_available_functions[j] = Distance_array_notifications_type_available[j]
                                                        + Distance_array_notifications_value_available[j];
    
    // Length Indicator Element - Platzhalter
    if (Distance_array_notifications_available_functions[j] > 0)
    {
      start_pos = payload_counter;
      payload_counter = payload_counter + 4;
    }
    else 
      start_pos = payload_counter;
    
    
    // 0 type (enum32)
    if (Distance_array_notifications_type_available[j] == 1)
    {
      tag_dataID = 0;
      Distance_payload_buffer_notifications[payload_counter++] = 2 * 16 /* Wiretype = 2 =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Distance_payload_buffer_notifications[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      Distance_payload_buffer_notifications[payload_counter++] = Distance_array_notifications_type[j]>>24 & 0xFF;
      Distance_payload_buffer_notifications[payload_counter++] = Distance_array_notifications_type[j]>>16 & 0xFF;
      Distance_payload_buffer_notifications[payload_counter++] = Distance_array_notifications_type[j]>> 8 & 0xFF;
      Distance_payload_buffer_notifications[payload_counter++] = Distance_array_notifications_type[j]     & 0xFF;
    }
    
    
    // 1 value (uint32)
    if (Distance_array_notifications_value_available[j] == 1)
    {
      tag_dataID = 1;
      Distance_payload_buffer_notifications[payload_counter++] = 2 * 16 /* Wiretype = 2 =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Distance_payload_buffer_notifications[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      Distance_payload_buffer_notifications[payload_counter++] = Distance_array_notifications_value[j]>>24 & 0xFF;
      Distance_payload_buffer_notifications[payload_counter++] = Distance_array_notifications_value[j]>>16 & 0xFF;
      Distance_payload_buffer_notifications[payload_counter++] = Distance_array_notifications_value[j]>> 8 & 0xFF;
      Distance_payload_buffer_notifications[payload_counter++] = Distance_array_notifications_value[j]     & 0xFF;
    }
    
    //Element Length
    element_length = payload_counter-start_pos;
    if (element_length >0) Distance_array_notifications_array_element_length[j] = element_length - 4;
    else                   Distance_array_notifications_array_element_length[j] = element_length;
    
    Distance_payload_buffer_notifications[start_pos++] = Distance_array_notifications_array_element_length[j]>>24 & 0xFF;
    Distance_payload_buffer_notifications[start_pos++] = Distance_array_notifications_array_element_length[j]>>16 & 0xFF;
    Distance_payload_buffer_notifications[start_pos++] = Distance_array_notifications_array_element_length[j]>> 8 & 0xFF;
    Distance_payload_buffer_notifications[start_pos++] = Distance_array_notifications_array_element_length[j]     & 0xFF;
    
    if (Distance_array_notifications_array_element_length[j] == 0) payload_counter = payload_counter+4;
  }
  
  // Struct Length
  Distance_array_notifications_struct_element_length = 4*@Distance_Notification_notifications_Element_TotalNum;

  for (k=0;k<@Distance_Notification_notifications_Element_TotalNum;k++)
   Distance_array_notifications_struct_element_length = Distance_array_notifications_struct_element_length + Distance_array_notifications_array_element_length[k];
  
  Distance_payload_buffer_notifications[0] = Distance_array_notifications_struct_element_length>>24 & 0xFF;
  Distance_payload_buffer_notifications[1] = Distance_array_notifications_struct_element_length>>16 & 0xFF;
  Distance_payload_buffer_notifications[2] = Distance_array_notifications_struct_element_length>> 8 & 0xFF;
  Distance_payload_buffer_notifications[3] = Distance_array_notifications_struct_element_length     & 0xFF;


  Distance_payload_length_notifications = Distance_array_notifications_struct_element_length+4;
  putvalue(Distance_Notification_notifications_Payload, Distance_payload_buffer_notifications, Distance_payload_length_notifications);
}


on envVar Distance_Notification_notifications_Element_TotalNum
{
  if (@this == 0) @this =  1;
  if (@this > 10) @this = 10;
  
  Distance_Notification_notifications();
  Distance_Notification_notifications_update();
}

on envVar Distance_Notification_notifications_Element
{
  Distance_Notification_notifications_update();
}

on envVar Distance_Notification_notifications_Element_Next
{
  if (@this == 1 && @Distance_Notification_notifications_Element < @Distance_Notification_notifications_Element_TotalNum - 1)
  {
    @Distance_Notification_notifications_Element = @Distance_Notification_notifications_Element + 1;
    Distance_Notification_notifications_update();
  }
}

on envVar Distance_Notification_notifications_Element_Previous
{
  if (@this == 1 && @Distance_Notification_notifications_Element > 0)
  {
    @Distance_Notification_notifications_Element = @Distance_Notification_notifications_Element - 1;
    Distance_Notification_notifications_update();
  }
}

Distance_Notification_notifications_update()
{
  @Distance_Notification_LengthIndicator_Element_notifications = Distance_array_notifications_array_element_length[@Distance_Notification_notifications_Element];
  @Distance_Notification_LengthIndicator_Struct_notifications  = Distance_array_notifications_struct_element_length;
  @Distance_Notification_Length_notifications                  = Distance_payload_length_notifications+8;
  
  @Distance_Notification_notifications_type  = Distance_array_notifications_type[@Distance_Notification_notifications_Element];
  @Distance_Notification_notifications_value = Distance_array_notifications_value[@Distance_Notification_notifications_Element];
  
  @Distance_Notification_notifications_type_available  = Distance_array_notifications_type_available[@Distance_Notification_notifications_Element];
  @Distance_Notification_notifications_value_available = Distance_array_notifications_value_available[@Distance_Notification_notifications_Element];
}










