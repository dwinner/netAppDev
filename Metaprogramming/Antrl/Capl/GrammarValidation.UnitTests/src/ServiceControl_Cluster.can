  word element_length  = 0;
  
  // Length Indicator Struct - Platzhalter
  payload_counter = 0+4;
  start_pos = 0;
  
  for (j=0; j<@Temperature_SetGet_temperatures_Element_TotalNum; j++)
  {
    Temperature_array_temperatures_available_functions[j] = Temperature_array_temperatures_type_available[j]
                                                          + Temperature_array_temperatures_currentValue_available[j]
                                                          + Temperature_array_temperatures_restrictionReason_available[j]
                                                          + Temperature_array_temperatures_unit_available[j]
																                          + Temperature_array_temperatures_minValue_available[j]
    																                      + Temperature_array_temperatures_maxValue_available[j]
    																                      + Temperature_array_temperatures_resolution_available[j]
                                                          + Temperature_array_temperatures_isValueInvalid_available[j];
    
    // Length Indicator Element - Platzhalter
    if (Temperature_array_temperatures_available_functions[j] > 0)
    {
      start_pos = payload_counter;
      payload_counter = payload_counter + 4;
    }
    else 
      start_pos = payload_counter;
    
    
    // 0 type (enum32)
    if (Temperature_array_temperatures_type_available[j] == 1)
    {
      tag_dataID = 0;
      Temperature_payload_buffer_temperatures[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Temperature_payload_buffer_temperatures[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_type[j]>>24 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_type[j]>>16 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_type[j]>> 8 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_type[j]     & 0xFF;
    }
    
    
    // 1 curentValue (float32)
    if (Temperature_array_temperatures_currentValue_available[j] == 1)
    {
      tag_dataID = 1;
      Temperature_payload_buffer_temperatures[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Temperature_payload_buffer_temperatures[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      temp_converttodword = interpretAsDword (Temperature_array_temperatures_currentValue[j]);
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword>>24 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword>>16 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword>> 8 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword     & 0xFF;
    }
    
    
    // 2 restrictionReason (enum32)
    if (Temperature_array_temperatures_restrictionReason_available[j] == 1)
    {
      tag_dataID = 2;
      Temperature_payload_buffer_temperatures[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Temperature_payload_buffer_temperatures[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_restrictionReason[j]>>24 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_restrictionReason[j]>>16 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_restrictionReason[j]>> 8 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_restrictionReason[j]     & 0xFF;
    }
    
    
    // 3 unit (enum32)
    if (Temperature_array_temperatures_unit_available[j] == 1)
    {
      tag_dataID = 3;
      Temperature_payload_buffer_temperatures[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
