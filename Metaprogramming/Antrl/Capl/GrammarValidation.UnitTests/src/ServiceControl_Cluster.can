on ethernetPacket *
{
  byte type;
  word length_of_entries_array;
  byte send_subAck_flag;
  
  byte  notification_rx_session_id;
  dword notification_rx_struct_length;
  dword notification_rx_element_length;
  dword someip_array_counter;
  
  if (this.type == 0x86DD /*&&  this.dir == RX*/) // IPv6       wieder einkommentieren, ist nur zum Test per IG!
  {
    // MAC Adresse auslesen und zuweisen
    //ethGetMacAddressAsString ( this.source, buffer_MAC, 18 );
    //putValue (ServiceDiscovery_EthernetPacket_MAC_Destination, buffer_MAC);
    
    if (this.ipv6.next == 17) // UDP
    {
      this.udp.GetData (0, someip_buffer, this.udp.length-8);
      
      // ServiceDiscovery
      if ((someip_buffer[0]&0x000000FF) == 0xFF && (someip_buffer[1]&0x000000FF) == 0xFF && (someip_buffer[2]&0x000000FF) == 0x81 && (someip_buffer[3]&0x000000FF) == 0x00)
      {
        // Lnge des Entry Arrays prfen und Anzahl der Elemente berechnen
        length_of_entries_array = (someip_buffer[20]&0x000000FF) * 16777216 + (someip_buffer[21]&0x000000FF) * 65536 + (someip_buffer[22]&0x000000FF) * 256 + someip_buffer[23]&0x000000FF;
        number_of_entries_array_elements = length_of_entries_array / 16;
        
        for (k=0;k<number_of_entries_array_elements;k++)
        {
          // Entries Array auswerten
          servicediscovery_rx_entries_array_type[k]              =   someip_buffer[24+16*k]     &0x000000FF;
          servicediscovery_rx_entries_array_index_1st_options[k] =   someip_buffer[25+16*k]     &0x000000FF;
          servicediscovery_rx_entries_array_index_2nd_options[k] =   someip_buffer[26+16*k]     &0x000000FF;
          servicediscovery_rx_entries_array_nr_of_opt_1[k]       =  (someip_buffer[27+16*k]>>4) &0x0000000F;
          servicediscovery_rx_entries_array_nr_of_opt_1[k]       =   someip_buffer[27+16*k]     &0x0000000F;
          servicediscovery_rx_entries_array_service_id[k]        = ((someip_buffer[28+16*k]     &0x000000FF) * 256)
                                                                 +  (someip_buffer[29+16*k]     &0x000000FF);
          servicediscovery_rx_entries_array_instance_id[k]       = ((someip_buffer[30+16*k]     &0x000000FF) * 256)
                                                                 +  (someip_buffer[31+16*k]     &0x000000FF);
          servicediscovery_rx_entries_array_major_version[k]     =   someip_buffer[32+16*k]     &0x000000FF;
          servicediscovery_rx_entries_array_ttl[k]               = ((someip_buffer[33+16*k]     &0x000000FF) * 65536)
                                                                 + ((someip_buffer[34+16*k]     &0x000000FF) * 256)
                                                                 +  (someip_buffer[35+16*k]     &0x000000FF);
          servicediscovery_rx_entries_array_eventgroup_id[k]     = ((someip_buffer[38+16*k]     &0x000000FF) * 256)
                                                                 +  (someip_buffer[39+16*k]     &0x000000FF);
          
          
          // Subscribe
          if (servicediscovery_rx_entries_array_type[k] == 6)
          {
            send_subAck_flag = 1; // wenn 1x Subscribe drin, dann ACK senden
          }
          
          
          // Subscribe Ack/NACK
          if (servicediscovery_rx_entries_array_type[k] == 7)
          {
            get_subAck_flag = 1;
            
            // UnitMaster_units
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x600E
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC010)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                UnitMaster_SubscribeStatus_units = 1;
              else
                UnitMaster_SubscribeStatus_units = 0;
            }
            
            // DisplaySpeed_speeds
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6021  
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC010)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                DisplaySpeed_SubscribeStatus_speeds = 1;
              else
                DisplaySpeed_SubscribeStatus_speeds = 0;
            }
            
            // DisplaySpeed_speedWarnings
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6021 
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC020)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                DisplaySpeed_SubscribeStatus_speedWarnings = 1;
              else
                DisplaySpeed_SubscribeStatus_speedWarnings = 0;
            }
            
            // DisplaySpeed_speedBlockings
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6021
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC030)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                DisplaySpeed_SubscribeStatus_speedBlockings = 1;
              else
                DisplaySpeed_SubscribeStatus_speedBlockings = 0;
            }
            
            // Temperature_temperatures
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6034
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC010)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                Temperature_SubscribeStatus_temperatures = 1;
              else
                Temperature_SubscribeStatus_temperatures = 0;
            }
            
            // Temperature_temperatureWarnings
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6034
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC020)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                Temperature_SubscribeStatus_temperatureWarnings = 1;
              else
                Temperature_SubscribeStatus_temperatureWarnings = 0;
            }
            
            // DrivingStatistics_speeds
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6035 
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC010)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                DrivingStatistics_SubscribeStatus_speeds = 1;
              else
                DrivingStatistics_SubscribeStatus_speeds = 0;
            }
            
            // DrivingStatistics_speedWarnings
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6035
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC020)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 2)
                DrivingStatistics_SubscribeStatus_distances = 1;
              else
                DrivingStatistics_SubscribeStatus_distances = 0;
            }
            
            // DrivingStatistics_speedBlockings
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6035
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC030)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                DrivingStatistics_SubscribeStatus_durations = 1;
              else
                DrivingStatistics_SubscribeStatus_durations = 0;
            }
            
            // DiagnosticNotification_notifications
            if (servicediscovery_rx_entries_array_service_id[k]    == 0x6046
            &&  servicediscovery_rx_entries_array_eventgroup_id[k] == 0xC010)
            {
              if (servicediscovery_rx_entries_array_ttl[k] > 0)
                DiagnosticNotification_SubscribeStatus_notifications = 1;
              else
                DiagnosticNotification_SubscribeStatus_notifications = 0;
            }
            
          }
          
          
          
          // Offer
          if ((someip_buffer[24+16*k]&0x000000FF) == 1)
          {
            servicediscovery_sub_tx_entries_array_service_id[k] = ((someip_buffer[28+16*k]&0x000000FF) * 256) + (someip_buffer[29+16*k]&0x000000FF);
            
            if (servicediscovery_sub_tx_entries_array_service_id[k] == 0x0600E)
            {
              @UnitMaster_available = 1;
              cancelTimer(UnitMaster_available_reset);
              setTimer(UnitMaster_available_reset, 1000);
            }
            if (servicediscovery_sub_tx_entries_array_service_id[k] == 0x06021)
            {
              @DisplaySpeed_available = 1;
              cancelTimer(DisplaySpeed_available_reset);
              setTimer(DisplaySpeed_available_reset, 1000);
            }
            if (servicediscovery_sub_tx_entries_array_service_id[k] == 0x06034)
            {
              @Temperature_available = 1;
              cancelTimer(Temperature_available_reset);
              setTimer(Temperature_available_reset, 1000);
            }
            if (servicediscovery_sub_tx_entries_array_service_id[k] == 0x06035)
            {
              @DrivingStatistics_available = 1;
              cancelTimer(DrivingStatistics_available_reset);
              setTimer(DrivingStatistics_available_reset, 1000);
            }
            if (servicediscovery_sub_tx_entries_array_service_id[k] == 0x06046)
            {
              @DiagnosticNotification_available = 1;
              cancelTimer(DiagnosticNotification_available_reset);
              setTimer(DiagnosticNotification_available_reset, 1000);
            }
          }
        }
        
        if (send_subAck_flag == 1)
        {
          send_subAck_flag = 0;
          
          this.ipv6.destination.GetData (0, buffer_ipv6, 16);
          
          if (buffer_ipv6[14] == 0x00 & buffer_ipv6[15] == 0x10)
            Send_ServiceDiscovery_SubscribeACK_ICAS1Gateway();
          if (buffer_ipv6[14] == 0x01 & buffer_ipv6[15] == 0x06)
            Send_ServiceDiscovery_SubscribeACK_ICAS1Sys01();
          if (buffer_ipv6[14] == 0x01 & buffer_ipv6[15] == 0x08)
            Send_ServiceDiscovery_SubscribeACK_ICAS3IVI();
        }
        
        // SubAck prfen
        if (get_subAck_flag == 1)
        {
          @UnitMaster_Subscribe_units                     = UnitMaster_SubscribeStatus_units;
          @DisplaySpeed_Subscribe_speeds                  = DisplaySpeed_SubscribeStatus_speeds;
          @DisplaySpeed_Subscribe_speedWarnings           = DisplaySpeed_SubscribeStatus_speedWarnings;
          @DisplaySpeed_Subscribe_speedBlockings          = DisplaySpeed_SubscribeStatus_speedBlockings;
          @Temperature_Subscribe_temperatures             = Temperature_SubscribeStatus_temperatures;
          @Temperature_Subscribe_temperatureWarnings      = Temperature_SubscribeStatus_temperatureWarnings;
          @DrivingStatistics_Subscribe_speeds             = DrivingStatistics_SubscribeStatus_speeds;
          @DrivingStatistics_Subscribe_distances          = DrivingStatistics_SubscribeStatus_distances;
          @DrivingStatistics_Subscribe_durations          = DrivingStatistics_SubscribeStatus_durations;
          @DiagnosticNotification_Subscribe_notifications = DiagnosticNotification_SubscribeStatus_notifications;
          
          settimer(get_subAck_flag_reset, 10);
        }
