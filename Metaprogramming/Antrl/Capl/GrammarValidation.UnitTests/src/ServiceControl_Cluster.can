UnitMaster_units_Notification()
{
  word data_length;
  word data_length_element_sum;
  word data_length_element[10];
  byte wiretype;
  byte data_id;
  word start_pos;
  
  payload_rx_counter = 0;
  data_length_element_sum = 0;
  UnitMaster_units_data_length_element_num = 0;
  UnitMaster_units_available_reset();
  
  // Gesamtlaenge
  data_length = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
  
  // pruefen ob noch Elemente vorhanden sind
  while (data_length_element_sum < data_length)
  {
    // Laenge Element
    data_length_element[UnitMaster_units_data_length_element_num] = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
                                                                  +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
                                                                  +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
                                                                  +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
    
    start_pos = payload_rx_counter;
    
    while (payload_rx_counter < data_length_element[UnitMaster_units_data_length_element_num] + start_pos)
    {
      // wiretype & data_id pruefen
      wiretype = (someip_payload_buffer[payload_rx_counter++]>>4) & 0x0F;
      data_id = someip_payload_buffer[payload_rx_counter++];
      
      switch (data_id)
      {
        case 0:  UnitMaster_Notification_units_id(); break;
        case 1:  UnitMaster_Notification_units_name(); break;
        case 2:  UnitMaster_Notification_units_dateFormat(); break;
        case 3:  UnitMaster_Notification_units_dateFormatConfiguration(wiretype); break;
        case 4:  UnitMaster_Notification_units_distanceUnit(); break;
        case 5:  UnitMaster_Notification_units_distanceUnitConfiguration(wiretype); break;
        case 6:  UnitMaster_Notification_units_pressureUnit(); break;
        case 7:  UnitMaster_Notification_units_pressureUnitConfiguration(wiretype); break;
        case 8:  UnitMaster_Notification_units_restrictionReason(); break;
        case 9:  UnitMaster_Notification_units_speedUnitTachometer(); break;
        case 10: UnitMaster_Notification_units_speedUnit(); break;
        case 11: UnitMaster_Notification_units_speedUnitConfiguration(wiretype); break;
        case 12: UnitMaster_Notification_units_temperatureUnit(); break;
        case 13: UnitMaster_Notification_units_temperatureUnitConfiguration(wiretype); break;
        case 14: UnitMaster_Notification_units_timeFormat(); break;
        case 15: UnitMaster_Notification_units_timeFormatConfiguration(wiretype); break;
        case 16: UnitMaster_Notification_units_volumeUnit(); break;
        case 17: UnitMaster_Notification_units_volumeUnitConfiguration(wiretype); break;
        case 18: UnitMaster_Notification_units_consumptionUnit(); break;
