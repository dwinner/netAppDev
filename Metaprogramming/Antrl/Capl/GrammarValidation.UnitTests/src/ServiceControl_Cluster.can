      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword>> 8 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword     & 0xFF;
    }
    
    
    // 7 isCodedStrict (boolean)
    if (Temperature_array_temperatures_isValueInvalid_available[j] == 1)
    {
      tag_dataID = 7;
      Temperature_payload_buffer_temperatures[payload_counter++] = 0 * 16 /* Wiretype = 0  =^ 8 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      Temperature_payload_buffer_temperatures[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      Temperature_payload_buffer_temperatures[payload_counter++] = Temperature_array_temperatures_isValueInvalid[j] & 0xFF;
    }
    
    
    //Element Length
    element_length = payload_counter-start_pos;
    if (element_length >0) Temperature_array_temperatures_array_element_length[j] = element_length - 4;
    else                   Temperature_array_temperatures_array_element_length[j] = element_length;
    
    Temperature_payload_buffer_temperatures[start_pos++] = Temperature_array_temperatures_array_element_length[j]>>24 & 0xFF;
    Temperature_payload_buffer_temperatures[start_pos++] = Temperature_array_temperatures_array_element_length[j]>>16 & 0xFF;
    Temperature_payload_buffer_temperatures[start_pos++] = Temperature_array_temperatures_array_element_length[j]>> 8 & 0xFF;
    Temperature_payload_buffer_temperatures[start_pos++] = Temperature_array_temperatures_array_element_length[j]     & 0xFF;
    
    if (Temperature_array_temperatures_array_element_length[j] == 0) payload_counter = payload_counter+4;
  }
  
  // Struct Length
  Temperature_array_temperatures_struct_element_length = 4*@Temperature_SetGet_temperatures_Element_TotalNum;

  for (k=0;k<@Temperature_SetGet_temperatures_Element_TotalNum;k++)
    Temperature_array_temperatures_struct_element_length = Temperature_array_temperatures_struct_element_length + Temperature_array_temperatures_array_element_length[k];
  
  Temperature_payload_buffer_temperatures[0] = Temperature_array_temperatures_struct_element_length>>24 & 0xFF;
  Temperature_payload_buffer_temperatures[1] = Temperature_array_temperatures_struct_element_length>>16 & 0xFF;
