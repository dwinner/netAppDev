      temp_converttodword = interpretAsDword (Temperature_array_temperatures_minValue[j]);
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword>>24 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword>>16 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword>> 8 & 0xFF;
      Temperature_payload_buffer_temperatures[payload_counter++] = temp_converttodword     & 0xFF;
    }
    
    
    // 5 maxValue (float32)
    if (Temperature_array_temperatures_maxValue_available[j] == 1)
    {
      tag_dataID = 5;
      Temperature_payload_buffer_temperatures[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
