    if (DisplaySpeed_array_speedBlockings_array_element_length[j] == 0) payload_counter = payload_counter+4;
  }
  
  // Struct Length
  DisplaySpeed_array_speedBlockings_struct_element_length = 4*@DisplaySpeed_SetGet_speedBlockings_Element_TotalNum;

  for (k=0;k<@DisplaySpeed_SetGet_speedBlockings_Element_TotalNum;k++)
    DisplaySpeed_array_speedBlockings_struct_element_length = DisplaySpeed_array_speedBlockings_struct_element_length + DisplaySpeed_array_speedBlockings_array_element_length[k];
  
  DisplaySpeed_payload_buffer_speedBlockings[0] = DisplaySpeed_array_speedBlockings_struct_element_length>>24 & 0xFF;
