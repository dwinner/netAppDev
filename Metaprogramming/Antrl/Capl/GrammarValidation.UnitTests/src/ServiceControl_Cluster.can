      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_restrictionReason[j]>> 8 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_restrictionReason[j]     & 0xFF;
    }
    
    
    //Element Length
    element_length = payload_counter-start_pos;
    if (element_length >0) DisplaySpeed_array_speedBlockings_array_element_length[j] = element_length - 4;
    else                   DisplaySpeed_array_speedBlockings_array_element_length[j] = element_length;
    
    DisplaySpeed_payload_buffer_speedBlockings[start_pos++] = DisplaySpeed_array_speedBlockings_array_element_length[j]>>24 & 0xFF;
