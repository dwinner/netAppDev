  }
  
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Notification_Request_ID_Session_ID>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Notification_Request_ID_Session_ID    & 0xFF;
  
  // SOME/IP Packet - Protocol Version [8 bit]
  buffer[i++] = @UnitMaster_SetGet_Protocol_Version;
  
  // SOME/IP Packet - Interface Version [8 bit]
  buffer[i++] = @UnitMaster_SetGet_Interface_Version;
  
  // SOME/IP Packet - Message Type [8 bit]
  buffer[i++] = @UnitMaster_SetGet_Message_Type;
  
  // SOME/IP Packet - Return Code [8 bit]
  buffer[i++] = @UnitMaster_SetGet_Return_Code;
  
  
  if (UDP_Segments_total > 1)
  {
    if (UDP_Segments_left >= 0)
    { 
      // Message Type neu setzen
      buffer[i-2] = @UnitMaster_SetGet_Message_Type + 0x20;
      
      // Offset (16byte Schritte, 1392/16 = 87)
      @UnitMaster_SetGet_Offset = 87 * (UDP_Segments_total - UDP_Segments_left);
      
      // More Segments Flag
      if (UDP_Segments_left > 1) @UnitMaster_SetGet_More_Segments_Flag = 1;
      else                       @UnitMaster_SetGet_More_Segments_Flag = 0;
      
      buffer[i++] =   @UnitMaster_SetGet_Offset>>20 & 0xFF;
      buffer[i++] =   @UnitMaster_SetGet_Offset>>12 & 0xFF;
      buffer[i++] =   @UnitMaster_SetGet_Offset>>4  & 0xFF;
      buffer[i++] = ((@UnitMaster_SetGet_Offset<<4) & 0xF0) + (@UnitMaster_SetGet_More_Segments_Flag & 0x01);
    }
  }
  
  UDP_Segments_left--;
  
  ////// units setter //////
  if (MethodID == 0x4011)
  {
    if (UDP_Segments_total == 1)
    {
      for (j=0; j<UnitMaster_payload_length_units; j++)
        buffer[i++] = UnitMaster_payload_buffer_units[j];
    }
    else
    {
      if (UDP_Segments_left >= 1)
      {
        for (j=(UDP_Segments_total-UDP_Segments_left-1)*1392; j<(UDP_Segments_total-UDP_Segments_left)*1392; j++)
          buffer[i++] = UnitMaster_payload_buffer_units[j];
      }
      else
      {
        for (j=(UDP_Segments_total-(UDP_Segments_left+1))*1392; j<UnitMaster_payload_length_units; j++)
          buffer[i++] = UnitMaster_payload_buffer_units[j];
      }
    }
  }
    
  // Rohdaten in UDP Paket schreiben
  EthPaket.udp.SetData( 0, buffer, i );

  // calculate UDP and IPv6 checksum and send Ethernet packet
  EthPaket.CompletePacket();
  output( EthPaket );
  
  // prfen ob alle Segemente gesendet wurden, sonst senden erneut aufrufen
  UnitMaster_Segmentation_Method_ID = MethodID;
  if (UDP_Segments_left>0)
    settimer(UnitMaster_Segmentation_delay, 50);
}

on timer UnitMaster_Segmentation_delay
{
  Send_Service_UnitMaster_SetGet (UnitMaster_Segmentation_Method_ID);
}






