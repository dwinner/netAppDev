DisplaySpeed_SetGet_speedBlockings()
{
  word payload_counter = 0;
  word start_pos       = 0;
  word element_length  = 0;
  
  // Length Indicator Struct - Platzhalter
  payload_counter = 0+4;
  start_pos = 0;
  
  for (j=0; j<@DisplaySpeed_SetGet_speedBlockings_Element_TotalNum; j++)
  {
    DisplaySpeed_array_speedBlockings_available_functions[j] = DisplaySpeed_array_speedBlockings_type_available[j]
                                                             + DisplaySpeed_array_speedBlockings_isActivated_available[j]
                                                             + DisplaySpeed_array_speedBlockings_isCodedStrict_available[j]
                                                             + DisplaySpeed_array_speedBlockings_threshold_available[j]
																	                           + DisplaySpeed_array_speedBlockings_unit_available[j]
                                                             + DisplaySpeed_array_speedBlockings_restrictionReason_available[j];
    
    // Length Indicator Element - Platzhalter
    if (DisplaySpeed_array_speedBlockings_available_functions[j] > 0)
    {
      start_pos = payload_counter;
      payload_counter = payload_counter + 4;
    }
    else 
      start_pos = payload_counter;
    
    
    // 0 type (enum32)
    if (DisplaySpeed_array_speedBlockings_type_available[j] == 1)
    {
      tag_dataID = 0;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_type[j]>>24 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_type[j]>>16 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_type[j]>> 8 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_type[j]     & 0xFF;
    }
    
    
    // 1 isActivated (boolean)
    if (DisplaySpeed_array_speedBlockings_isActivated_available[j] == 1)
    {
      tag_dataID = 1;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = 0 * 16 /* Wiretype = 0  =^ 8 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_isActivated[j] & 0xFF;
    }
    
    
    // 2 isCodedStrict (boolean)
    if (DisplaySpeed_array_speedBlockings_restrictionReason_available[j] == 1)
    {
      tag_dataID = 2;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = 0 * 16 /* Wiretype = 0  =^ 8 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_isCodedStrict[j] & 0xFF;
    }
    
    
    // 3 threshold (uint32)
    if (DisplaySpeed_array_speedBlockings_threshold_available[j] == 1)
    {
      tag_dataID = 3;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_threshold[j]>>24 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_threshold[j]>>16 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_threshold[j]>> 8 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_threshold[j]     & 0xFF;
    }
    
    
    // 4 unit (enum32)
    if (DisplaySpeed_array_speedBlockings_unit_available[j] == 1)
    {
      tag_dataID = 4;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_unit[j]>>24 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_unit[j]>>16 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_unit[j]>> 8 & 0xFF;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_unit[j]     & 0xFF;
    }
    
    
    // 5 restrictionReason (enum)
    if (DisplaySpeed_array_speedBlockings_restrictionReason_available[j] == 1)
    {
      tag_dataID = 5;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DisplaySpeed_payload_buffer_speedBlockings[payload_counter++] = DisplaySpeed_array_speedBlockings_restrictionReason[j]>>24 & 0xFF;
