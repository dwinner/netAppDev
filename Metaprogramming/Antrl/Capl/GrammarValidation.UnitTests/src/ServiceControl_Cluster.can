DrivingStatistics_SetGet_speeds()
{
  word payload_counter = 0;
  word start_pos       = 0;
  word element_length  = 0;
  
  // Length Indicator Struct - Platzhalter
  payload_counter = 0+4;
  start_pos = 0;
  
  for (j=0; j<@DrivingStatistics_SetGet_speeds_Element_TotalNum; j++)
  {
    DrivingStatistics_array_speeds_available_functions[j] = DrivingStatistics_array_speeds_memoryLevel_available[j]
                                                          + DrivingStatistics_array_speeds_currentValue_available[j]
                                                          + DrivingStatistics_array_speeds_state_available[j];
    
    // Length Indicator Element - Platzhalter
    if (DrivingStatistics_array_speeds_available_functions[j] > 0)
    {
      start_pos = payload_counter;
      payload_counter = payload_counter + 4;
    }
    else 
      start_pos = payload_counter;
    
    
    // 0 memoryLevel (uint32)
    if (DrivingStatistics_array_speeds_memoryLevel_available[j] == 1)
    {
      tag_dataID = 0;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = DrivingStatistics_array_speeds_memoryLevel[j]>>24 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = DrivingStatistics_array_speeds_memoryLevel[j]>>16 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = DrivingStatistics_array_speeds_memoryLevel[j]>> 8 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = DrivingStatistics_array_speeds_memoryLevel[j]     & 0xFF;
    }
    
    // 1 currentValue (float32)
    if (DrivingStatistics_array_speeds_currentValue_available[j] == 1)
    {
      tag_dataID = 1;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      temp_converttodword = interpretAsDword (DrivingStatistics_array_speeds_currentValue[j]);
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = temp_converttodword>>24 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = temp_converttodword>>16 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = temp_converttodword>> 8 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = temp_converttodword     & 0xFF;
    }
    
    
    // 2 state (enum32)
    if (DrivingStatistics_array_speeds_state_available[j] == 1)
    {
      tag_dataID = 2;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = DrivingStatistics_array_speeds_state[j]>>24 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = DrivingStatistics_array_speeds_state[j]>>16 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = DrivingStatistics_array_speeds_state[j]>> 8 & 0xFF;
      DrivingStatistics_payload_buffer_speeds[payload_counter++] = DrivingStatistics_array_speeds_state[j]     & 0xFF;
    }
    
    
    //Element Length
    element_length = payload_counter-start_pos;
    if (element_length >0) DrivingStatistics_array_speeds_array_element_length[j] = element_length - 4;
    else                   DrivingStatistics_array_speeds_array_element_length[j] = element_length;
    
    DrivingStatistics_payload_buffer_speeds[start_pos++] = DrivingStatistics_array_speeds_array_element_length[j]>>24 & 0xFF;
    DrivingStatistics_payload_buffer_speeds[start_pos++] = DrivingStatistics_array_speeds_array_element_length[j]>>16 & 0xFF;
    DrivingStatistics_payload_buffer_speeds[start_pos++] = DrivingStatistics_array_speeds_array_element_length[j]>> 8 & 0xFF;
    DrivingStatistics_payload_buffer_speeds[start_pos++] = DrivingStatistics_array_speeds_array_element_length[j]     & 0xFF;
    
    if (DrivingStatistics_array_speeds_array_element_length[j] == 0) payload_counter = payload_counter+4;
  }
  
  // Struct Length
  DrivingStatistics_array_speeds_struct_element_length = 4*@DrivingStatistics_SetGet_speeds_Element_TotalNum;

  for (k=0;k<@DrivingStatistics_SetGet_speeds_Element_TotalNum;k++)
    DrivingStatistics_array_speeds_struct_element_length = DrivingStatistics_array_speeds_struct_element_length + DrivingStatistics_array_speeds_array_element_length[k];
  
  DrivingStatistics_payload_buffer_speeds[0] = DrivingStatistics_array_speeds_struct_element_length>>24 & 0xFF;
  DrivingStatistics_payload_buffer_speeds[1] = DrivingStatistics_array_speeds_struct_element_length>>16 & 0xFF;
  DrivingStatistics_payload_buffer_speeds[2] = DrivingStatistics_array_speeds_struct_element_length>> 8 & 0xFF;
  DrivingStatistics_payload_buffer_speeds[3] = DrivingStatistics_array_speeds_struct_element_length     & 0xFF;


  DrivingStatistics_payload_length_speeds = DrivingStatistics_array_speeds_struct_element_length+4;
  putvalue(DrivingStatistics_SetGet_speeds_Payload, DrivingStatistics_payload_buffer_speeds, DrivingStatistics_payload_length_speeds);
}

