DiagnosticNotification_SetGet_notifications()
{
  word payload_counter = 0;
  word start_pos       = 0;
  word element_length  = 0;
  
  // Length Indicator Struct - Platzhalter
  payload_counter = 0+4;
  start_pos = 0;
  
  for (j=0; j<@DiagnosticNotification_SetGet_notifications_Element_TotalNum; j++)
  {
    DiagnosticNotification_array_notifications_available_functions[j] = DiagnosticNotification_array_notifications_listId_available[j]
                                                                      + DiagnosticNotification_array_notifications_notificationIds_available[j];
    
    // Length Indicator Element - Platzhalter
    if (DiagnosticNotification_array_notifications_available_functions[j] > 0)
    {
      start_pos = payload_counter;
      payload_counter = payload_counter + 4;
    }
    else 
      start_pos = payload_counter;
    
    
    // 0 type (enum32)
    if (DiagnosticNotification_array_notifications_listId_available[j] == 1)
    {
      tag_dataID = 0;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = 2 * 16 /* Wiretype = 2  =^ 32 Bit */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_listId[j]>>24 & 0xFF;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_listId[j]>>16 & 0xFF;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_listId[j]>> 8 & 0xFF;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_listId[j]     & 0xFF;
    }
    
    // 1 notificationIds (int32 array)
    if (DiagnosticNotification_array_notifications_notificationIds_available[j] == 1)
    {
      tag_dataID = 1;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = 7 * 16 /* Wiretype = 7 =^ Array mit 4 Byte Lnge */ + ((tag_dataID>>8) & 0x0F) /* DataID High Part */ ;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = tag_dataID & 0xFF /* DataID Low Part */ ;
      
      // Array Length
      DiagnosticNotification_array_notifications_notificationIds_length = 4 * DiagnosticNotification_array_notifications_notificationIds_nr_of_elements[j];
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_notificationIds_length>>24 & 0xFF;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_notificationIds_length>>16 & 0xFF;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_notificationIds_length>> 8 & 0xFF;
      DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_notificationIds_length     & 0xFF;
      
      for (k=0; k<DiagnosticNotification_array_notifications_notificationIds_nr_of_elements[j]; k++)
      {
        DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_notificationIds[j][k]>>24 & 0xFF;
        DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_notificationIds[j][k]>>16 & 0xFF;
        DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_notificationIds[j][k]>> 8 & 0xFF;
        DiagnosticNotification_payload_buffer_notifications[payload_counter++] = DiagnosticNotification_array_notifications_notificationIds[j][k]     & 0xFF;
      }
    }
    
    
    //Element Length
    element_length = payload_counter-start_pos;
    if (element_length >0) DiagnosticNotification_array_notifications_array_element_length[j] = element_length - 4;
    else                   DiagnosticNotification_array_notifications_array_element_length[j] = element_length;
    
    DiagnosticNotification_payload_buffer_notifications[start_pos++] = DiagnosticNotification_array_notifications_array_element_length[j]>>24 & 0xFF;
    DiagnosticNotification_payload_buffer_notifications[start_pos++] = DiagnosticNotification_array_notifications_array_element_length[j]>>16 & 0xFF;
    DiagnosticNotification_payload_buffer_notifications[start_pos++] = DiagnosticNotification_array_notifications_array_element_length[j]>> 8 & 0xFF;
    DiagnosticNotification_payload_buffer_notifications[start_pos++] = DiagnosticNotification_array_notifications_array_element_length[j]     & 0xFF;
    
    if (DiagnosticNotification_array_notifications_array_element_length[j] == 0) payload_counter = payload_counter+4;
  }
  
  // Struct Length
  DiagnosticNotification_array_notifications_struct_element_length = 4*@DiagnosticNotification_SetGet_notifications_Element_TotalNum;

  for (k=0;k<@DiagnosticNotification_SetGet_notifications_Element_TotalNum;k++)
    DiagnosticNotification_array_notifications_struct_element_length = DiagnosticNotification_array_notifications_struct_element_length + DiagnosticNotification_array_notifications_array_element_length[k];
  
  DiagnosticNotification_payload_buffer_notifications[0] = DiagnosticNotification_array_notifications_struct_element_length>>24 & 0xFF;
  DiagnosticNotification_payload_buffer_notifications[1] = DiagnosticNotification_array_notifications_struct_element_length>>16 & 0xFF;
  DiagnosticNotification_payload_buffer_notifications[2] = DiagnosticNotification_array_notifications_struct_element_length>> 8 & 0xFF;
  DiagnosticNotification_payload_buffer_notifications[3] = DiagnosticNotification_array_notifications_struct_element_length     & 0xFF;


  DiagnosticNotification_payload_length_notifications = DiagnosticNotification_array_notifications_struct_element_length+4;
  putvalue(DiagnosticNotification_SetGet_notifications_Payload, DiagnosticNotification_payload_buffer_notifications, DiagnosticNotification_payload_length_notifications);
}

