Send_Service_DrivingStatistics_SetGet (word MethodID)
{
  ethernetPacket EthPaket;

  // Ethernet Packet - MAC Source address
  getvalue (ServiceDiscovery_EthernetPacket_MAC_Source_ICAS1Sys02, buffer_MAC);
  //EthPaket.source.ParseAddress ( buffer_MAC );
  EthPaket.source = ethGetMacAddressAsNumber (buffer_MAC);
  
  // Ethernet Packet - MAC Destination address
  getvalue (ServiceDiscovery_EthernetPacket_MAC_Destination, buffer_MAC);
  //EthPaket.destination.ParseAddress ( buffer_MAC );
  EthPaket.destination = ethGetMacAddressAsNumber (buffer_MAC);
  
  // Ethernet Packet - VLAN
  EthPaket.SetVlan (@ServiceDiscovery_EthernetPacket_VLAN_TPID, @ServiceDiscovery_EthernetPacket_VLAN_TCI);
  
  // Ethernet Packet - Type
  //EthPaket.ipv6.type = @EthernetPacket_Type;
  if (@ServiceDiscovery_EthernetPacket_Type == 0x86DD) EthPaket.ipv6.Init();
  
  
  // IPv6 Packet - Version
  EthPaket.ipv6.version = @ServiceDiscovery_IPv6Packet_Version;

  // IPv6 Packet - Traffic Class
  EthPaket.ipv6.class = @ServiceDiscovery_IPv6Packet_Traffic_Class;
  
  // IPv6 Packet - Flow Label
  EthPaket.ipv6.flow = @ServiceDiscovery_IPv6Packet_Flow_Label;
  
  // IPv6 Packet - Payload_Length
  EthPaket.ipv6.length = @ServiceDiscovery_IPv6Packet_Payload_Length;
  
  // IPv6 Packet - Next Header
  EthPaket.ipv6.next = @ServiceDiscovery_IPv6Packet_Next_Header;
  if (@ServiceDiscovery_IPv6Packet_Next_Header == 6) EthPaket.tcp.Init();
  if (@ServiceDiscovery_IPv6Packet_Next_Header == 17) EthPaket.udp.Init();
  
  // IPv6 Packet - Hop Limit
  EthPaket.ipv6.hopLimit = @ServiceDiscovery_IPv6Packet_Hop_Limit;

  // IPv6 Packet - IPv6 Destination address    // TODO: char array zu byte array
  buffer_ipv6[0]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_01>>8 & 0xFF;
  buffer_ipv6[1]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_01    & 0xFF;
  buffer_ipv6[2]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_02>>8 & 0xFF;
  buffer_ipv6[3]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_02    & 0xFF;
  buffer_ipv6[4]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_03>>8 & 0xFF;
  buffer_ipv6[5]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_03    & 0xFF;
  buffer_ipv6[6]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_04>>8 & 0xFF;
  buffer_ipv6[7]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_04    & 0xFF;
  buffer_ipv6[8]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_05>>8 & 0xFF;
  buffer_ipv6[9]  = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_05    & 0xFF;
  buffer_ipv6[10] = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_06>>8 & 0xFF;
  buffer_ipv6[11] = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_06    & 0xFF;
  buffer_ipv6[12] = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_07>>8 & 0xFF;
  buffer_ipv6[13] = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_07    & 0xFF;
  buffer_ipv6[14] = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_08>>8 & 0xFF;
  buffer_ipv6[15] = @ServiceDiscovery_IPv6Packet_IP_Destination_Unicast_08    & 0xFF;
  EthPaket.ipv6.destination.SetData ( 0, buffer_ipv6, elcount(buffer_ipv6) );

  // IPv6 Packet - IPv6 Source address    // TODO: char array zu byte array
  buffer_ipv6[0]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_01>>8 & 0xFF;
  buffer_ipv6[1]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_01    & 0xFF;
  buffer_ipv6[2]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_02>>8 & 0xFF;
  buffer_ipv6[3]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_02    & 0xFF;
  buffer_ipv6[4]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_03>>8 & 0xFF;
  buffer_ipv6[5]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_03    & 0xFF;
  buffer_ipv6[6]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_04>>8 & 0xFF;
  buffer_ipv6[7]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_04    & 0xFF;
  buffer_ipv6[8]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_05>>8 & 0xFF;
  buffer_ipv6[9]  = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_05    & 0xFF;
  buffer_ipv6[10] = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_06>>8 & 0xFF;
  buffer_ipv6[11] = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_06    & 0xFF;
  buffer_ipv6[12] = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_07>>8 & 0xFF;
  buffer_ipv6[13] = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_07    & 0xFF;
  buffer_ipv6[14] = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_08>>8 & 0xFF;
  buffer_ipv6[15] = @ServiceDiscovery_IPv6Packet_IP_Source_ICAS1Sys02_08    & 0xFF;
  EthPaket.ipv6.source.SetData ( 0, buffer_ipv6, elcount(buffer_ipv6) );

  
  // UDP Packet - Source Port
  EthPaket.udp.source = @DrivingStatistics_SetGet_Port_Source;
  
  // UDP Packet - Destination Port
  EthPaket.udp.destination = @DrivingStatistics_SetGet_Port_Destination;
  
  // UDP Packet - Length
  EthPaket.udp.length = @ServiceDiscovery_UDPPacket_Length;
  
  
  // SOME/IP Packet aus Rohdaten zusammenbauen (buffer array)
  i=0;
  
  // SOME/IP Packet - Message ID, Service ID [16 bit]
  buffer[i++] = @DrivingStatistics_SetGet_Message_ID_Service_ID>>8 & 0xFF;
  buffer[i++] = @DrivingStatistics_SetGet_Message_ID_Service_ID    & 0xFF;
  
  // SOME/IP Packet - Message ID, Message ID [16 bit]
  switch (MethodID)
  {
    case 0x4010: @DrivingStatistics_SetGet_Message_ID_Method_ID = @DrivingStatistics_SetGet_Message_ID_Method_ID_speeds_Get; break;
    case 0x4011: @DrivingStatistics_SetGet_Message_ID_Method_ID = @DrivingStatistics_SetGet_Message_ID_Method_ID_speeds_Set; break;
    case 0x4020: @DrivingStatistics_SetGet_Message_ID_Method_ID = @DrivingStatistics_SetGet_Message_ID_Method_ID_distances_Get; break;
    case 0x4021: @DrivingStatistics_SetGet_Message_ID_Method_ID = @DrivingStatistics_SetGet_Message_ID_Method_ID_distances_Set; break;
    case 0x4030: @DrivingStatistics_SetGet_Message_ID_Method_ID = @DrivingStatistics_SetGet_Message_ID_Method_ID_durations_Get; break;
    case 0x4031: @DrivingStatistics_SetGet_Message_ID_Method_ID = @DrivingStatistics_SetGet_Message_ID_Method_ID_durations_Set; break;
  }
  buffer[i++] = @DrivingStatistics_SetGet_Message_ID_Method_ID>>8 & 0xFF;
  buffer[i++] = @DrivingStatistics_SetGet_Message_ID_Method_ID    & 0xFF;
  
  // SOME/IP Packet - Length [32 bit]
  if (UDP_Segments_total == 1)
  {
    switch (MethodID)
    {
      case 0x4010: @DrivingStatistics_SetGet_Length = 8; break;
      case 0x4011: @DrivingStatistics_SetGet_Length = @DrivingStatistics_SetGet_Length_speeds; break;
      case 0x4020: @DrivingStatistics_SetGet_Length = 8; break;
      case 0x4021: @DrivingStatistics_SetGet_Length = @DrivingStatistics_SetGet_Length_distances; break;
      case 0x4030: @DrivingStatistics_SetGet_Length = 8; break;
      case 0x4031: @DrivingStatistics_SetGet_Length = @DrivingStatistics_SetGet_Length_durations; break;
    }
  }
  else
  {
    if (UDP_Segments_left > 1)
    {
      switch (MethodID)
      {
        case 0x4021: @DrivingStatistics_SetGet_Length = 1404; break;
      }
    }
    else
    {
      switch (MethodID)
      {
        case 0x4011: @DrivingStatistics_SetGet_Length = DrivingStatistics_payload_length_speeds    - 1404*(UDP_Segments_total-UDP_Segments_left) + 24; break;
        case 0x4021: @DrivingStatistics_SetGet_Length = DrivingStatistics_payload_length_distances - 1404*(UDP_Segments_total-UDP_Segments_left) + 24; break;
        case 0x4031: @DrivingStatistics_SetGet_Length = DrivingStatistics_payload_length_durations - 1404*(UDP_Segments_total-UDP_Segments_left) + 24; break;
      }
    }
  }
  buffer[i++] = @DrivingStatistics_SetGet_Length>>24 & 0xFF;
  buffer[i++] = @DrivingStatistics_SetGet_Length>>16 & 0xFF;
  buffer[i++] = @DrivingStatistics_SetGet_Length>> 8 & 0xFF;
  buffer[i++] = @DrivingStatistics_SetGet_Length     & 0xFF;
  
  // SOME/IP Packet - Request ID, Client ID [16 bit]
  buffer[i++] = @DrivingStatistics_SetGet_Request_ID_Client_ID>>8 & 0xFF;
  buffer[i++] = @DrivingStatistics_SetGet_Request_ID_Client_ID    & 0xFF;
  
  // SOME/IP Packet - Request ID, Session ID) [16 bit]
  if ((UDP_Segments_total == 1) || (seg_id_flag == 1))
  {
    @ServiceDiscovery_SOMEIPPacket_Notification_Request_ID_Session_ID = @ServiceDiscovery_SOMEIPPacket_Notification_Request_ID_Session_ID + 1;
    if (@ServiceDiscovery_SOMEIPPacket_Notification_Request_ID_Session_ID == 0x10000) @ServiceDiscovery_SOMEIPPacket_Notification_Request_ID_Session_ID = 1;
    seg_id_flag = 0;
  }
  
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Notification_Request_ID_Session_ID>>8 & 0xFF;
  buffer[i++] = @ServiceDiscovery_SOMEIPPacket_Notification_Request_ID_Session_ID    & 0xFF;
  
  // SOME/IP Packet - Protocol Version [8 bit]
  buffer[i++] = @DrivingStatistics_SetGet_Protocol_Version;
  
  // SOME/IP Packet - Interface Version [8 bit]
  buffer[i++] = @DrivingStatistics_SetGet_Interface_Version;
  
  // SOME/IP Packet - Message Type [8 bit]
  buffer[i++] = @DrivingStatistics_SetGet_Message_Type;
  
  // SOME/IP Packet - Return Code [8 bit]
  buffer[i++] = @DrivingStatistics_SetGet_Return_Code;
  
  
  if (UDP_Segments_total > 1)
  {
    if (UDP_Segments_left >= 0)
    { 
      // Message Type neu setzen
      buffer[i-2] = @DrivingStatistics_SetGet_Message_Type + 0x20;
      
      // Offset (16byte Schritte, 1392/16 = 87)
      @DrivingStatistics_SetGet_Offset = 87 * (UDP_Segments_total - UDP_Segments_left);
      
      // More Segments Flag
      if (UDP_Segments_left > 1) @DrivingStatistics_SetGet_More_Segments_Flag = 1;
      else                       @DrivingStatistics_SetGet_More_Segments_Flag = 0;
      
      buffer[i++] =   @DrivingStatistics_SetGet_Offset>>20 & 0xFF;
      buffer[i++] =   @DrivingStatistics_SetGet_Offset>>12 & 0xFF;
      buffer[i++] =   @DrivingStatistics_SetGet_Offset>>4  & 0xFF;
      buffer[i++] = ((@DrivingStatistics_SetGet_Offset<<4) & 0xF0) + (@DrivingStatistics_SetGet_More_Segments_Flag & 0x01);
    }
  }
  
  UDP_Segments_left--;
  
  ////// speeds setter //////
  if (MethodID == 0x4011)
  {
    if (UDP_Segments_total == 1)
    {
      for (j=0; j<DrivingStatistics_payload_length_speeds; j++)
        buffer[i++] = DrivingStatistics_payload_buffer_speeds[j];
    }
    else
    {
      if (UDP_Segments_left >= 1)
      {
        for (j=(UDP_Segments_total-UDP_Segments_left-1)*1392; j<(UDP_Segments_total-UDP_Segments_left)*1392; j++)
          buffer[i++] = DrivingStatistics_payload_buffer_speeds[j];
      }
      else
      {
        for (j=(UDP_Segments_total-(UDP_Segments_left+1))*1392; j<DrivingStatistics_payload_length_speeds; j++)
          buffer[i++] = DrivingStatistics_payload_buffer_speeds[j];
      }
    }
  }
  
  ////// distances setter //////
  if (MethodID == 0x4021)
  {
    if (UDP_Segments_total == 1)
    {
      for (j=0; j<DrivingStatistics_payload_length_distances; j++)
        buffer[i++] = DrivingStatistics_payload_buffer_distances[j];
    }
    else
    {
      if (UDP_Segments_left >= 1)
      {
        for (j=(UDP_Segments_total-UDP_Segments_left-1)*1392; j<(UDP_Segments_total-UDP_Segments_left)*1392; j++)
          buffer[i++] = DrivingStatistics_payload_buffer_distances[j];
      }
      else
      {
        for (j=(UDP_Segments_total-(UDP_Segments_left+1))*1392; j<DrivingStatistics_payload_length_distances; j++)
          buffer[i++] = DrivingStatistics_payload_buffer_distances[j];
      }
    }
  }
  
  ////// durations setter //////
  if (MethodID == 0x4031)
  {
    if (UDP_Segments_total == 1)
    {
      for (j=0; j<DrivingStatistics_payload_length_durations; j++)
        buffer[i++] = DrivingStatistics_payload_buffer_durations[j];
    }
    else
    {
      if (UDP_Segments_left >= 1)
      {
        for (j=(UDP_Segments_total-UDP_Segments_left-1)*1392; j<(UDP_Segments_total-UDP_Segments_left)*1392; j++)
          buffer[i++] = DrivingStatistics_payload_buffer_durations[j];
      }
      else
      {
        for (j=(UDP_Segments_total-(UDP_Segments_left+1))*1392; j<DrivingStatistics_payload_length_durations; j++)
          buffer[i++] = DrivingStatistics_payload_buffer_durations[j];
      }
    }
  }
    
  // Rohdaten in UDP Paket schreiben
  EthPaket.udp.SetData( 0, buffer, i );

  // calculate UDP and IPv6 checksum and send Ethernet packet
  EthPaket.CompletePacket();
  output( EthPaket );
  
  // prfen ob alle Segemente gesendet wurden, sonst senden erneut aufrufen
  DrivingStatistics_Segmentation_Method_ID = MethodID;
  if (UDP_Segments_left>0)
    settimer(DrivingStatistics_Segmentation_delay, 50);
}

on timer DrivingStatistics_Segmentation_delay
{
  Send_Service_DrivingStatistics_SetGet (DrivingStatistics_Segmentation_Method_ID);
}






