DrivingStatistics_durations_Notification()
{
  word data_length;
  word data_length_element_sum;
  word data_length_element[10];
  byte wirememoryLevel;
  byte data_id;
  word start_pos;
  
  payload_rx_counter = 0;
  data_length_element_sum = 0;
  DrivingStatistics_durations_data_length_element_num = 0;
  DrivingStatistics_durations_available_reset();
  
  // Gesamtlaenge
  data_length = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
              +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
  
  // pruefen ob noch Elemente vorhanden sind
  while (data_length_element_sum < data_length)
  {
    // Laenge Element
    data_length_element[DrivingStatistics_durations_data_length_element_num] = ((someip_payload_buffer[payload_rx_counter++]&0x000000FF)*16777216
                                                                          +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*65536
                                                                          +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF)*256
                                                                          +  (someip_payload_buffer[payload_rx_counter++]&0x000000FF));
   
    start_pos = payload_rx_counter;
    
    while (payload_rx_counter < data_length_element[DrivingStatistics_durations_data_length_element_num] + start_pos)
    {
      // wirememoryLevel & data_id pruefen
      wirememoryLevel = (someip_payload_buffer[payload_rx_counter++]>>4) & 0x0F;
      data_id = someip_payload_buffer[payload_rx_counter++];
      
      switch (data_id)
      {
        case 0:  DrivingStatistics_Notification_durations_memoryLevel(); break;
        case 1:  DrivingStatistics_Notification_durations_currentValue(); break;
        case 2:  DrivingStatistics_Notification_durations_state(); break;
      }
    }
    data_length_element_sum = data_length_element_sum + data_length_element[DrivingStatistics_durations_data_length_element_num] + 4;
    
    if (data_length_element_sum < data_length)
      DrivingStatistics_durations_data_length_element_num++;
  }
  @DrivingStatistics_SetGet_durations_Element_TotalNum = DrivingStatistics_durations_data_length_element_num+1;
}

