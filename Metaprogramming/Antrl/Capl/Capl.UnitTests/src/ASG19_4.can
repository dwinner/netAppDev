A_ASG_19_1_0_28_SetGet_GenerateSetGet()
{
    byte head[5];
    //Record Address in Byte 0 Bit 0-3 kopieren - MAX Wert = 0x0F
    if(getValue(A_ASG_19_1_0_28_SetGet_RecAdr) > 0x0F)
        putValue(A_ASG_19_1_0_28_SetGet_RecAdr , 0x0F);
    head[0] = (head[0] & 0xF0) | getValue(A_ASG_19_1_0_28_SetGet_RecAdr);
    //Shift Bit in Byte 0 Bit 4 kopieren
    if( getValue(A_ASG_19_1_0_28_SetGet_Shift) > 0) head[0] = head[0] | 0x10;
    else head[0] = head[0] & 0xEF;
    // Direction Bit in Byte 0 Bit 5 kopieren
    if( getValue(A_ASG_19_1_0_28_SetGet_Direction) > 0) head[0] = head[0] | 0x20;
    else head[0] = head[0] & 0xDF;
    // reserverd:  Byte 0 Bit 6
    if( getValue(A_ASG_19_1_0_28_SetGet_IndexSize) > 0)
    {
        // 5 Byte Header
        head[0] = head[0] | 0x80;
        head[1] =  getValue(A_ASG_19_1_0_28_SetGet_Start) & 0xFF;
        head[2] = (getValue(A_ASG_19_1_0_28_SetGet_Start) >> 8 ) & 0xFF;
        head[3] =  getValue(A_ASG_19_1_0_28_SetGet_Elements) & 0xFF;
        head[4] = (getValue(A_ASG_19_1_0_28_SetGet_Elements)  >> 8 ) & 0xFF;
        //ToDo: DATA - derzeit einfach laenge = 5
        putValue(A_ASG_19_1_0_28_SetGet, head, 5); 
    }
    else 
    {
        // 3 Byte Header
        head[0] = head[0] & 0x7F;
        head[1] = getValue(A_ASG_19_1_0_28_SetGet_Start) & 0xFF;
        head[2] = getValue(A_ASG_19_1_0_28_SetGet_Elements) & 0xFF;
        //ToDo: DATA - derzeit einfach laenge = 3
        putValue(A_ASG_19_1_0_28_SetGet, head, 3); 
    }
}

