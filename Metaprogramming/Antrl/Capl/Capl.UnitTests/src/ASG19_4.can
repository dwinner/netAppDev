	thisval = thisval&~(1<<4)|(bitval<<4);
	putValue(P_ASG_19_1_0_13_1,thisval);
}

on envVar P_ASG_19_1_0_13_1 // FSG_Control.Controlcode
{
	byte superval;

	//Update Childvalue (BitField)
	byte mask=0x01;
	int i = 0;
	byte thisval; // Controlcode
	thisval = getValue(this);
	if(thisval&(mask<<0))
		putValue(P_ASG_19_1_0_13_1_0, 1);
	else
		putValue(P_ASG_19_1_0_13_1_0, 0);
	if(thisval&(mask<<4))
		putValue(P_ASG_19_1_0_13_1_4, 1);
	else
		putValue(P_ASG_19_1_0_13_1_4, 0);

    // Update superval
	superval = getValue(P_ASG_19_1_0_13);
	superval = thisval;
	putValue(P_ASG_19_1_0_13,superval);
}

