on envVar P_ASG_15_1_0_14 // bus value of FSG_Setup (14).
{
	// opcodes: Error-Status
	word param1; // DisplayInstallation1
	byte param2; // DisplayInstallation2
	byte param3; // PrimaryEngineType
	byte param4; // SecondaryEngineType
	byte param5; // FunctionSupport
	byte param6; // Extension1
	// DATATYPE_BYTESEQUENCE / DATATYPE_FIXEDBYTESEQUENCE
	int i;
	byte superval[6];
	int currentPos;
	currentPos = 0;
	for( i = 0; i < 6; i++ ) {
		superval[i] = 0;
	}
	getValue(P_ASG_15_1_0_14, superval);
	///////// Start param1 - UINT16 /////////
	param1 = superval[currentPos++]|(superval[currentPos++]<<8);
	putValue(P_ASG_15_1_0_14_1, param1);
	///////// Start param2 - UINT8 /////////
	param2 = superval[currentPos++];
	putValue(P_ASG_15_1_0_14_2, param2);
	///////// Start param3 - NIBBLE /////////
	param3 = (superval[currentPos] & 0xf0)>>4;
	putValue(P_ASG_15_1_0_14_3, param3);
	///////// Start param4 - NIBBLE /////////
	param4 = superval[currentPos++] & 0x0f;
	putValue(P_ASG_15_1_0_14_4, param4);
	///////// Start param5 - UINT8 /////////
	param5 = superval[currentPos++];
	putValue(P_ASG_15_1_0_14_5, param5);
	///////// Start param6 - UINT8 /////////
	param6 = superval[currentPos++];
	putValue(P_ASG_15_1_0_14_6, round((param6 * 1.0) - (0) + (0), 1.0));
}

