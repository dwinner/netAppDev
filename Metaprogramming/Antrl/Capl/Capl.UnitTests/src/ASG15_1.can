on envVar A_ASG_15_1_0_31Get_BTN
{
  byte data[6]; 
  byte stringValue[6];
  byte head[5];
  int i;
  int headLength;
  long tempValue;
  word currentPos;
  word startVal;
  word elementsVal;
  byte nibbleSetted;
  if (A_ASG_15_1_0_31Get_BTN_already_pressed == 0)
  {
     putValue(A_ASG_15_1_0_31_ErrCode,0);
     SetControlBackColor(panel_name,"errField_A_ASG_15_1_0_31",MakeRGB(255,255,255));
     A_ASG_15_1_0_31Get_BTN_already_pressed = 1;
  }
  else
  {
        currentPos = 0;
        //Einzelparameter: ASG_ID
        tempValue = getValue(A_ASG_15_1_0_31_1_SetGet);
    if(nibbleSetted == 1)
    {
        //voriger Parameter war ein Nibble, => second nibble, Pos incr und Flag zuruecksetzen
        data[currentPos] = (tempValue &0x0F) | (data[currentPos] &0xF0);
        currentPos++;
        nibbleSetted = 0;
    }
    else
    {
        //first nibble, kein increment, Flag setzen
        data[currentPos] = (tempValue<<4) &0xF0;
        nibbleSetted = 1;
    }
        //Einzelparameter: TAID
        tempValue = getValue(A_ASG_15_1_0_31_2_SetGet);
    if(nibbleSetted == 1)
    {
        //voriger Parameter war ein Nibble, => second nibble, Pos incr und Flag zuruecksetzen
        data[currentPos] = (tempValue &0x0F) | (data[currentPos] &0xF0);
        currentPos++;
        nibbleSetted = 0;
    }
    else
    {
        //first nibble, kein increment, Flag setzen
        data[currentPos] = (tempValue<<4) &0xF0;
        nibbleSetted = 1;
    }
        //head Parameter
        headLength = getValueSize(A_ASG_15_1_0_31_SetGet);
        getValue(A_ASG_15_1_0_31_SetGet,head);
        for(i=0; i< headLength; i++)
        {
            data[currentPos++] = head[i];
        }

     Request_ByteSequence(LSGID, A_ASG_15_1_0_31_ID, DataGet_REQ, data, currentPos);
     if(getValue(A_ASG_15_1_0_31_IntTlgEn)) //getValue(IT))
     {
       intertelegram_err(A_ASG_15_1_0_31_ID);
     }
     if(getValue(A_ASG_15_1_0_31_SegmEn))   //getvalue(SG)
     {
         sequence_err(A_ASG_15_1_0_31_ID,getValueSize(A_ASG_15_1_0_31_SetGet));
     }
     A_ASG_15_1_0_31Get_BTN_already_pressed = 0;
  }
}
A_ASG_15_1_0_31_SetGet_GenerateSetGet()
{
    byte head[5];
    //Record Address in Byte 0 Bit 0-3 kopieren - MAX Wert = 0x0F
    if(getValue(A_ASG_15_1_0_31_SetGet_RecAdr) > 0x0F)
        putValue(A_ASG_15_1_0_31_SetGet_RecAdr , 0x0F);
    head[0] = (head[0] & 0xF0) | getValue(A_ASG_15_1_0_31_SetGet_RecAdr);
    //Shift Bit in Byte 0 Bit 4 kopieren
    if( getValue(A_ASG_15_1_0_31_SetGet_Shift) > 0) head[0] = head[0] | 0x10;
    else head[0] = head[0] & 0xEF;
    // Direction Bit in Byte 0 Bit 5 kopieren
